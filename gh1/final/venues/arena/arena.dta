{func hit_speakers
   ($speakers)
   {foreach $anim $speakers
      {arena switch_anim
         $anim
         (range {random_int 110 130} 0)
         (scale 1)
         (blend 0)
      }
   }
}
{func hit_guitar_amp
   ($slot)
   {hit_speakers (stack_speakers.tnm stack_speakers02.tnm)}
}
