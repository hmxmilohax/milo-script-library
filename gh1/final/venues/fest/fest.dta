{arena load_section lighting lighting}
{arena load_section crowd crowd}
#define OFF (
   (loop 99999 99999)
   (scale 1)
   (blend 0)
)
{set $reactorState FALSE}
{func set_lights_bad
   {arena switch_anim verse.anim OFF}
   {arena switch_anim chorus.anim OFF}
   {arena switch_anim solo.anim OFF}
   {arena switch_anim lightGR.anim OFF}
   {arena switch_anim lightOK.anim OFF}
   {arena switch_anim venue_main.anim (loop 60000 65000) (scale 1) (blend 0)}
   {arena switch_anim bad.anim (loop 60000 65000) (scale 1) (blend 0)}
   {arena switch_anim full.anim (loop 60000 65000) (scale 1) (blend 0)}
   {arena set_singer_env singer0.env}
}
{func set_lights_okay_verse
   {arena switch_anim bad.anim OFF}
   {arena switch_anim chorus.anim OFF}
   {arena switch_anim solo.anim OFF}
   {arena switch_anim lightGR.anim OFF}
   {arena switch_anim full.anim (loop 30000 35000) (scale 1) (blend 0)}
   {arena switch_anim verse.anim (loop 30000 35000) (scale 1) (blend 0)}
   {arena switch_anim venue_main.anim (loop 30000 35000) (scale 1) (blend 0)}
   {arena switch_anim lightOK.anim (loop 30000 35000) (scale 1) (blend 0)}
   {arena set_singer_env singer0.env}
}
