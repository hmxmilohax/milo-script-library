{new_gh_screen qp_selsong
   (panels meta sel_song helpbar)
   (focus sel_song)
   (helpbar
      (default
         (
            (fret1 help_continue)
            (fret2 help_back)
            (strum help_updown)
         )
      )
   )
   (sel_song_career FALSE)
   (SELECT_MSG
      {meta setup_quickplay}
      {ui goto_screen qp_diff}
      kDataUnhandled
   )
}
{new_gh_screen qp_diff
   (panels meta sel_difficulty helpbar)
   (focus sel_difficulty)
   (helpbar
      (default
         (
            (fret1 help_continue)
            (fret2 help_back)
            (strum help_updown)
         )
      )
   )
   (SELECT_MSG
      {game set_quickplay}
      {game set_multiplayer FALSE}
      {ui goto_screen loading}
      kDataUnhandled
   )
}