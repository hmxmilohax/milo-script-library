                     $text
                     "[mellow"}
                  {has_substr
                     $text
                     "[intense"}}}
            {set $playTick $tick}
            {set
               $newMIE
               {array
                  ({sprint
                        "playstart_"
                        {elem $focus_names $focusID}}
                     $focusArr
                     {-
                        $tick
                        {'*' 480 $BONUS_INTENSITY_BEFORE_BEATS}}
                     {'+'
                        $tick
                        {'*' 480 $BONUS_INTENSITY_AFTER_BEATS}}
                     2.5)}}
            {push_back $MIEEvents $newMIE}}
         {if
            {'||'
               {has_substr
                  $text
                  "[mellow"}
               {has_substr
                  $text
                  "[intense"}}
            {set
               $newMIE
               {array
                  ({sprint
                        "intensitychange_"
                        {elem $focus_names $focusID}}
                     $focusArr
                     {-
                        $tick
                        {'*' 480 $BONUS_INTENSITY_BEFORE_BEATS}}
                     {'+'
                        $tick
                        {'*' 480 $BONUS_INTENSITY_AFTER_BEATS}}
                     2.5)}}
            {push_back $MIEEvents $newMIE}}
         {if
            {has_substr
               $text
               "[idle"}
            {unless
               {== $playTick -1}
               {set
                  $newMIE
                  {array
                     ({sprint
                           "playing_"
                           {elem $focus_names $focusID}}
                        $focusArr
                        $playTick
                        $tick
                        1.0)}}
               {push_back $MIEEvents $newMIE}}
            {set $playTick -1}}}}
