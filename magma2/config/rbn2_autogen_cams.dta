{func
   CheckShouldAutogenCams
   {set
      [working_track]
      "VENUE"}
   {set $hasCamCues FALSE}
   {$this
      foreach_textevent
      $text
      $tick
      {if
         {'||'
            {has_substr
               $text
               "coop_"}
            {has_substr
               $text
               "directed_"}}
         {set $hasCamCues TRUE}}}
   {set
      $willAutogen
      {'||'
         [force_autogen]
         {! $hasCamCues}}}
   {unless
      $willAutogen
      {$this add_symbol_key shot_5 coop_all_far 0}}
   $willAutogen}
{func
   GetCamPacingFromSymbol
   ($pacingName)
   {do
      ($result kCamIntervalMed)
      {switch
         $pacingName
         (minimal
            {set $result kCamIntervalMinimal})
         (slow
            {set $result kCamIntervalSlow})
         (medium
            {set $result kCamIntervalMed})
         (fast
            {set $result kCamIntervalFast})
         (crazy
            {set $result kCamIntervalCrazy})}
      $result}}
{func
   UpdateMinTicksBetweenCuts
   ($camInterval)
   {if_else
      {>= $camInterval kCamIntervalFast}
      {switch
         $camInterval
         (kCamIntervalFast
            {set $MIN_TICKS_BETWEEN_CUTS 480})
         (kCamIntervalCrazy
            {set $MIN_TICKS_BETWEEN_CUTS 1})}
      {set $MIN_TICKS_BETWEEN_CUTS 960}}}
{set $CAMAUTOGEN_ENABLE_PRIMARY_FOCUSES 1}
{set $RANDOMIZED_CAM_INTERVAL kCamIntervalMed}
{set
   $themePacingArr
   {$this get_theme_def camera_pacing}}
{if
   {>
      {size $themePacingArr}
      1}
   {set
      $RANDOMIZED_CAM_INTERVAL
      {GetCamPacingFromSymbol
         {elem $themePacingArr 1}}}}
{if_else
   {== $RANDOMIZED_CAM_INTERVAL kCamIntervalMinimal}
   {set $CAMAUTOGEN_ENABLE_RANDOMIZED_CAMS 0}
   {set $CAMAUTOGEN_ENABLE_RANDOMIZED_CAMS 1}}
{UpdateMinTicksBetweenCuts $RANDOMIZED_CAM_INTERVAL}
{set $PROB_PRIMARY_FOCUS_ONLY 20}
{set $PROB_PRIMARY_PLUS_OTHERS 45}
{set $PROB_NO_PRIMARY_FOCUS_SINGLE 38}
{set $PROB_ONE_OR_MORE 50}
{set $PROB_SINGLE_FOCUS 50}
{set $BONUS_INTENSITY_BEFORE_BEATS 2}
{set $BONUS_INTENSITY_AFTER_BEATS 4}
