(songs_dta
   "e:/p4root/band/band/run/songs/songs.dta")
(process_single
   {set
      $results
      {array 0}}
   {print
      "[PROCESSING] "
      {file_get_base
         [datamine_file]}
      "\n"}
   {set
      [working_track]
      "PART GUITAR"}
   {$this
      foreach_midievent
      kNote_G6
      kNote_G6
      $pitch
      $tick
      $state
      {if_else
         $state
         {do
            {print
               "solo found!\n"}
            {push_back
               $results
               {array
                  ("guitar_solo"
                     {file_get_base
                        [datamine_file]})}}}
         {do
            {print
               "solo ended\n"}}}}
   $results)
(tabulate_results
   {print
      "\n\n[tabulate_results]\n"}
   {$this
      foreach_datamine_result
      $result
      {foreach_int
         $i
         0
         {size $result}
         {set
            $curRes
            {elem $result $i}}
         {print
            {elem $curRes 1}
            "\n"}}})