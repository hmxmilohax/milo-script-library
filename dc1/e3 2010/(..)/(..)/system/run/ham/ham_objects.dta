                  (help
                     "Symbolic name of target"))
               (target_ptr
                  object
                  (class Trans)
                  (read_only 1)
                  (help
                     "pointer to target"))
               (anim_group
                  symbol
                  (list
                     {do
                        ($drv
                           {find_obj
                              [targets
                                 {elem $milo_prop_path 1}
                                 target_ptr]
                              main.drv})
                        {object_list
                           {if_else
                              $drv
                              {$drv get clips}
                              ''}
                           CharClipGroup}})
