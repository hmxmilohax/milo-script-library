                  {'+=' $stars 1}
                  {do
                     {'+='
                        $stars
                        {/
                           {- $score $last_threshold}
                           {- $threshold $last_threshold}}}
                     {set $done 1}}}
               {set $last_threshold $threshold}}}
         $stars})
