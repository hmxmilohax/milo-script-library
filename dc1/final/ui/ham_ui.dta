#define COMMON_ERROR_HANDLERS
(COMMON_DIALOG_PANEL_HANDLERS
   (helpbar
      ((confirm helpbar_select))))
(ui_event_mgr
   (dialog_events
      (error_message
         COMMON_ERROR_HANDLERS
         (init
            ($token)
            {$this set_ok $token})
         (SELECT_DONE_MSG
            {ui_event_mgr dismiss_event}))
      (saveload_dialog_event
         COMMON_ERROR_HANDLERS
         (init
            {do
               ($msg
                  {saveload_mgr get_dialog_msg})
               ($opt1
                  {saveload_mgr get_dialog_opt1})
               ($opt2
                  {saveload_mgr get_dialog_opt2})
               ($opt3
                  {saveload_mgr get_dialog_opt3})
               ($focus_index
                  {saveload_mgr get_dialog_focus_option})
               ($focus opt1.btn)
               {set
                  $focus
                  {switch
                     $focus_index
                     (1 opt1.btn)
                     (2 opt2.btn)
                     (3 opt3.btn)}}
               {if_else
                  {!= $opt3 ''}
                  {$this set_custom_3btn $msg $opt1 $opt2 $opt3 $focus}
                  {if_else
                     {!= $opt2 ''}
                     {$this set_custom $msg $opt1 $opt2 $focus}
                     {$this set_ok $msg}}}})
         (SELECT_MSG
            {do
               ($choice
                  {switch
                     $component
                     (opt1.btn 1)
                     (ok.btn 1)
                     (yes.btn 1)
                     (opt2.btn 2)
                     (no.btn 2)
                     (opt3.btn 3)
                     1})
               {saveload_mgr handle_eventresponse_start $choice}})
         (SELECT_DONE_MSG
            {do
               ($choice
                  {switch
                     $component
                     (opt1.btn 1)
                     (ok.btn 1)
                     (yes.btn 1)
                     (opt2.btn 2)
                     (no.btn 2)
                     (opt3.btn 3)
                     1})
               {ui_event_mgr dismiss_event}
               {saveload_mgr
                  handle_eventresponse
                  {user_mgr get_active_user}
                  $choice}})))
   (transition_events
      (leaderboard_error
         (next_screen meta_loading_leaderboard_error_screen)
         (destination_screen
            {meta_loading_leaderboard_error_screen get next_screen})
         (init
            {ui_event_mgr
               trigger_event
               error_message
               (dummy init error_lost_leaderboards)}))
      (storage_changed
         (next_screen meta_loading_uisink_default_screen)
         (destination_screen
            {meta_loading_uisink_default_screen get next_screen})
         (init
            {ui_event_mgr
               trigger_event
               error_message
               (dummy init error_storage_devices_changed)}))
      (data_error
         (next_screen meta_loading_uisink_default_screen)
         (destination_screen
            {meta_loading_uisink_default_screen get next_screen})
         (init
            ($corrupt $info_str)
            {ui_event_mgr
               trigger_event
               error_message
               (dummy
                  init
                  {if_else
                     $corrupt
                     (error_corrupt_song_data $info_str)
                     error_lost_song_data})}))
      (sign_out
         (next_screen meta_loading_signout_screen)
         (destination_screen
            {meta_loading_signout_screen get next_screen})
         (init
            ($reason)
            {ui_event_mgr
               trigger_event
               error_message
               (dummy
                  init
                  {switch
                     $reason
                     (kLocalUserSignedOut error_local_user_signed_out)
                     (kProfileOwnerSignedOut error_profile_owner_signed_out)})}))
      (sign_in
         (next_screen meta_loading_signout_screen)
         (destination_screen
            {meta_loading_signout_screen get next_screen})
         (init
            ($reason)
            {ui_event_mgr
               trigger_event
               error_message
               (dummy init error_local_user_signed_out)}))
      (disc_error
         (next_screen meta_loading_uisink_lost_connection_screen)
         (destination_screen
            {meta_loading_uisink_lost_connection_screen get next_screen})
         (init
            {ui_event_mgr
               trigger_event
               error_message
               (dummy init error_disc)}))))