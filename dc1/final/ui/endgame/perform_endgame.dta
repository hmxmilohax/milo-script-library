{new
   HamPanel
   perform_endgame_panel
   (file
      "perform_endgame.milo")
   (original_leaderboards_mode 0)
   (show_hand_help FALSE)
   (is_failure FALSE)
   (focus none)
   (enter
      {unless
         {ui went_back}
         {meta_performer
            trigger_song_completion
            {$this get_score}
            {$this get_num_stars}}
         {meta_performer
            advance_song
            {$this get_num_stars}}}
      {if_else
         {&&
            {exists leaderboards}
            {!
               {profile_mgr is_playing_without_save_file}}}
         {do
            {set
               [original_leaderboards_mode]
               {leaderboards get_mode}}
            {leaderboards set_mode kLeaderboardsFriends}
            {leaderboards
               download_scores
               {meta_performer song}}
            {instarank.hbs set_provider leaderboards}
            {instarank.grp set showing TRUE}}
         {instarank.grp set showing FALSE}}
      {platform_mgr set_notify_ui_location kOSNotifyBottomCenter}
      {$this set_results}
      ENDGAME_PANEL_COMMON_ENTER
      {$this
         begin_results
         {$this get_num_stars}}
      {meta_performer add_sink $this}
      {if_else
         {meta_performer get_fitness_mode}
         {do
            {fitness_header.lbl set showing TRUE}
            {fitness_time.lbl set showing TRUE}
            {fitness_calories_total.lbl set showing TRUE}
            {fitness_time.lbl set_fitness_time}
            {fitness_calories_total.lbl set_fitness_total_calories}}
         {do
            {fitness_header.lbl set showing FALSE}
            {fitness_time.lbl set showing FALSE}
            {fitness_calories_total.lbl set showing FALSE}}})
   (exit
      {platform_mgr set_notify_ui_location kOSNotifyTopRight}
      {meta_performer remove_sink $this})
   (leaderboards_loaded
      {leaderboards
         insert_own_score
         {$this get_score}}
      {instarank.hbs update_list}
      {leaderboards
         set_mode
         [original_leaderboards_mode]})
   (get_score
      {int
         {game_panel get score}})
   (get_num_stars
      {int
         {game_panel get num_stars}})
   (set_results
      {song.lbl
         set_song_name
         {meta_performer get_completed_song}}
      {difficulty.lbl
         set
         text_token
         {meta_performer get_difficulty_sym}}
      {moves_passed.lbl
         set_token_fmt
         moves_passed
         {meta_performer get_moves_passed 0}})
   (do_score_countup
      {score_countup.lnt
         set
         desired_value
         {$this get_score}})
   (begin_results
      ($num_stars)
      {set
         [is_failure]
         {< $num_stars 3}}
      {stars_0.trg set callback_object $this}
      {stars_1.trg set callback_object $this}
      {stars_2.trg set callback_object $this}
      {stars_3.trg set callback_object $this}
      {stars_4.trg set callback_object $this}
      {stars_5.trg set callback_object $this}
      {stars_6.trg set callback_object $this}
      {no_flashcards.trg set callback_object $this}
      {new_best_stars.trg set callback_object $this}
      {show_score.trg set callback_object $this}
      {new_high_score.trg set callback_object $this}
      {show_progress_bar.trg set callback_object $this}
      {progress.rpb set callback_object $this}
      {shrink_score.trg set callback_object $this}
      {show_buttons.trg set callback_object $this}
      {unlock_medium.trg set callback_object $this}
      {unlock_expert.trg set callback_object $this}
      {reset.trg trigger}
      {$this show_stars $num_stars})
   (ui_trigger_complete
      ($trigger)
      {switch
         $trigger
         (stars_0.trg
            {$this show_new_best_stars})
         (stars_1.trg
            {$this show_new_best_stars})
         (stars_2.trg
            {$this show_new_best_stars})
         (stars_3.trg
            {$this show_new_best_stars})
         (stars_4.trg
            {$this show_new_best_stars})
         (stars_5.trg
            {$this show_new_best_stars})
         (stars_6.trg
            {$this show_new_best_stars})
         (no_flashcards.trg
            {$this show_new_best_stars})
         (new_best_stars.trg
            {$this show_progress_bar})
         (show_score.trg
            {$this show_no_flashcards})
         (new_high_score.trg
            {$this shrink_score})
         (show_progress_bar.trg
            {$this do_progress_bar})
         (shrink_score.trg
            {$this show_buttons})
         (show_buttons.trg
            {saveload_mgr autosave}
            {ui goto_screen perform_continue_screen}
            {$this show_unlocked_medium})
         (unlock_medium.trg
            {$this show_unlocked_expert})
         (unlock_expert.trg
            {$this show_unlocked_no_flashcards})})
   (rank_animation_complete
      ($progress_bar)
      {$this show_score})
   (show_stars
      ($num_stars)
      {switch
         $num_stars
         (0
            {stars_0.trg trigger})
         (1
            {stars_1.trg trigger})
         (2
            {stars_2.trg trigger})
         (3
            {stars_3.trg trigger})
         (4
            {stars_4.trg trigger})
         (5
            {stars_5.trg trigger})
         (6
            {stars_6.trg trigger})})
   (show_no_flashcards
      {if
         {meta_performer completed_song_with_no_flashcards}
         {no_flash_stars.trg trigger}
         {no_flash_score.trg trigger}}
      {$this show_new_high_score})
   (show_new_best_stars
      {if_else
         {&&
            {meta_performer got_new_best_stars}
            {!
               [is_failure]}}
         {new_best_stars.trg trigger}
         {$this show_progress_bar}})
   (show_score
      {show_score.trg trigger}
      {$this do_score_countup})
   (show_new_high_score
      {if_else
         {&&
            {meta_performer got_new_high_score}
            {!
               [is_failure]}}
         {new_high_score.trg trigger}
         {$this shrink_score}})
   (show_progress_bar
      {do
         ($profile
            {profile_mgr
               get_profile
               {user_mgr get_active_user}})
         ($rank
            {$profile get_rank})
         {progress.rpb
            set
            start_percent
            {$rank get_percent_to_next_rank_begin}}
         {progress.rpb
            set
            end_percent
            {$rank get_percent_to_next_rank_end}}
         {progress.rpb display_start}
         {if
            {$rank at_max_rank}
            {progress.rpb set at_max_rank TRUE}}}
      {show_progress_bar.trg trigger})
   (do_progress_bar
      {progress.rpb animate})
   (shrink_score
      {shrink_score.trg trigger})
   (show_buttons
      {show_buttons.trg trigger})
   (show_unlocked_medium
      {if_else
         {meta_performer unlocked_medium_difficulty}
         {unlock_medium.trg trigger}
         {$this show_unlocked_expert}})
   (show_unlocked_expert
      {if_else
         {meta_performer unlocked_expert_difficulty}
         {unlock_expert.trg trigger}
         {$this show_unlocked_no_flashcards}})
   (show_unlocked_no_flashcards
      {if
         {meta_performer unlocked_no_flashcards}
         {unlock_no_flash.trg trigger}})
   (continue_next_screen
      {if_else
         {meta_performer just_beat_game}
         {ui pop_screen beat_game_screen}
         {ui goto_screen perform_complete_screen}})}
{new
   HamPanel
   perform_continue_panel
   (file
      "../low_button.milo")
   (focus continue.btn)
   (hide_helpbar
      {!
         {shell_input in_controller_mode}})
   (helpbar_confirm_label continue)
   (show_hand_help FALSE)
   (enter
      {right_hand.hbs
         set
         showing
         {!
            {shell_input in_controller_mode}}})
   (SELECT_MSG
      {switch
         $component
         (continue.btn
            {do
               ($profile
                  {profile_mgr
                     get_profile
                     {user_mgr get_active_user}})
               ($rank
                  {$profile get_rank})
               {if_else
                  {'||'
                     {$rank has_new_rank}
                     {acc_mgr has_new_awards}}
                  {ui goto_screen perform_endgame_popup_between_screen}
                  {perform_endgame_panel continue_next_screen}}})})
   (controller_mode_entered
      {right_hand.hbs set showing FALSE})
   (controller_mode_exited
      {right_hand.hbs set showing TRUE})}
{new
   HamPanel
   perform_complete_panel
   (file
      "perform_complete.milo")
   (focus photos.btn)
   (enter
      {song.lbl
         set_song_name
         {meta_performer get_completed_song}}
      {difficulty.lbl
         set
         text_token
         {meta_performer get_difficulty_sym}}
      {if_else
         {acc_mgr
            is_unlockable_asset
            {meta_performer song}}
         {do
            {$this disable practice.btn}
            {shell_input update_button_zones}
            {practice_encourage.lbl set showing FALSE}}
         {if_else
            {<
               {perform_endgame_panel get_num_stars}
               3}
            {do
               {practice_encourage.lbl set showing TRUE}
               {practice_encourage.trig trigger}}
            {practice_encourage.lbl set showing FALSE}}}
      {if
         {meta_performer last_played_mode}
         {gamemode
            set_mode
            {meta_performer last_played_mode}}})
   (SELECT_MSG
      {switch
         $component
         (restart.btn
            {meta_performer restart}
            {game_restart})
         (practice.btn
            {gamemode set_mode practice}
            {{gamemode get ingame_seldiff_screen}
               set
               back_screen
               perform_complete_screen}
            {ui
               goto_screen
               {gamemode get ingame_seldiff_screen}})
         (multiplayer.btn
            {gamemode set_mode multiplayer}
            {{gamemode get ingame_seldiff_screen}
               set
               back_screen
               perform_complete_screen}
            {ui
               goto_screen
               {gamemode get ingame_seldiff_screen}})
         (change_difficulty.btn
            {{gamemode get ingame_seldiff_screen}
               set
               back_screen
               perform_complete_screen}
            {ui
               goto_screen
               {gamemode get ingame_seldiff_screen}})
         (photos.btn
            {endgame_photos_screen set back_screen perform_complete_screen}
            {ui goto_screen endgame_photos_screen})
         (continue.btn
            {ui pop_screen meta_loading_continue_screen})})}
{new
   HamScreen
   perform_endgame_screen
   (panels meta background_panel perform_endgame_panel)
   (focus perform_endgame_panel)}
{new
   HamScreen
   perform_continue_screen
   (panels meta background_panel perform_endgame_panel perform_continue_panel)
   (focus perform_continue_panel)}
{new
   HamPanel
   perform_endgame_popup_underneath
   (file empty_screen.milo)
   (current_popup none)
   (enter
      {set
         [current_popup]
         none})
   (TRANSITION_COMPLETE_MSG
      {switch
         [current_popup]
         (none
            {$this show_rank_popup})
         (new_rank
            {$this show_unlock_popup})
         (new_unlock
            {$this show_unlock_popup})})
   (show_rank_popup
      {do
         ($profile
            {profile_mgr
               get_profile
               {user_mgr get_active_user}})
         ($rank
            {$profile get_rank})
         {if_else
            {$rank has_new_rank}
            {do
               {set
                  [current_popup]
                  new_rank}
               {endgame_popup_panel
                  set_popup_text
                  endgame_new_rank
                  {$rank get_rank_title}
                  {$rank get_rank_description}}
               {$this show_popup}
               {switch
                  {$rank get_rank_title}
                  (rank_2
                     {acc_mgr
                        earn_accomplishment
                        {user_mgr get_active_user}
                        acc_passed_first_rank})
                  (rank_20
                     {acc_mgr
                        earn_accomplishment
                        {user_mgr get_active_user}
                        acc_reached_last_rank})}}
            {$this show_unlock_popup}}
         {$rank reset_progress}})
   (show_unlock_popup
      {if_else
         {acc_mgr has_new_awards}
         {do
            {set
               [current_popup]
               new_unlock}
            {endgame_popup_panel
               set_popup_text
               endgame_unlocked_content
               {acc_mgr
                  get_name_for_first_new_award
                  {user_mgr get_active_user}}
               {acc_mgr
                  get_description_for_first_new_award
                  {user_mgr get_active_user}}}
            {acc_mgr
               clear_first_new_award
               {user_mgr get_active_user}}
            {$this show_popup}
            {if
               {acc_mgr
                  is_group_complete
                  {profile_mgr
                     get_profile
                     {user_mgr get_active_user}}
                  acc_grp_unlocks}
               {acc_mgr
                  earn_accomplishment
                  {user_mgr get_active_user}
                  acc_unlocked_all_content}}
            {if
               {acc_mgr
                  is_category_complete
                  {profile_mgr
                     get_profile
                     {user_mgr get_active_user}}
                  acc_cat_costumes}
               {acc_mgr
                  earn_accomplishment
                  {user_mgr get_active_user}
                  acc_unlocked_all_costumes}}}
         {perform_endgame_panel continue_next_screen}})
   (show_popup
      {ui goto_screen perform_endgame_popup_screen})}
{new
   HamScreen
   perform_endgame_popup_between_screen
   (panels meta background_panel perform_endgame_popup_underneath)
   (focus perform_endgame_popup_underneath)}
{new
   HamScreen
   perform_endgame_popup_screen
   (panels meta background_panel perform_endgame_popup_underneath endgame_popup_panel)
   (focus endgame_popup_panel)}
{new
   HamScreen
   perform_complete_screen
   (panels meta background_panel perform_complete_panel)
   (focus perform_complete_panel)}