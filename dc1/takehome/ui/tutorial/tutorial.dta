{new
   HamPanel
   tutorial_next_panel
   (file
      "tutorial_next.milo")
   (focus next.btn)
   (on_complete options)
   (hide_helpbar
      {!
         {shell_input in_controller_mode}})
   (helpbar_confirm_label tutorial_next)
   (show_hand_help FALSE)
   (enter
      {if_else
         {==
            {{ui current_screen}
               get
               next_screen}
            none}
         {next.btn set text_token tutorial_done}
         {next.btn set text_token tutorial_next}}
      {right_hand.hbs
         set
         showing
         {!
            {shell_input in_controller_mode}}}
      {{cursor_panel find depth_buffer.grp}
         set
         showing
         FALSE})
   (exit)
   (SELECT_MSG
      {switch
         $component
         (next.btn
            {do
               ($next_screen
                  {{ui current_screen}
                     get
                     next_screen})
               {if_else
                  {== $next_screen none}
                  {switch
                     [on_complete]
                     (options
                        {ui goto_screen options_tutorials_screen})
                     (start_game
                        {enter_gameplay})}
                  {do
                     {tutorial_advance_screen set next_screen $next_screen}
                     {ui push_screen tutorial_advance_screen}}}})
         (back.btn
            {ui
               go_back_screen
               {{ui current_screen}
                  get
                  back}})})
   (controller_mode_entered
      {right_hand.hbs set showing FALSE})
   (controller_mode_exited
      {right_hand.hbs set showing TRUE})}
{new
   HamPanel
   tutorial_advance_panel
   (hide_helpbar
      {!
         {shell_input in_controller_mode}})
   (helpbar_confirm_label tutorial_next)}
{new
   HamScreen
   tutorial_advance_screen
   (panels tutorial_advance_panel)
   (focus tutorial_advance_panel)
   (next_screen none)
   (TRANSITION_COMPLETE_MSG
      {ui
         pop_screen
         [next_screen]})}
