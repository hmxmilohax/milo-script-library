{new
   HamScreen
   tutorial_perform_intro_screen
   (panels meta background_panel tutorial_perform_intro_panel tutorial_next_panel)
   (focus tutorial_next_panel)
   (next_screen
      {switch
         {tutorial_next_panel get on_complete}
         (options tutorial_passing_moves_1_screen)
         (start_game
            {if_else
               {profile_mgr has_seen_any_mode_tutorial}
               tutorial_flashcards_screen
               tutorial_passing_moves_1_screen})})}
