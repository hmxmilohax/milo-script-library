{func
   fake_win
   ($num_stars)
   {if_else
      {&&
         {exists game}
         {game_panel is_up}}
      {do
         {game_panel set num_stars $num_stars}
         {game_panel set score 666}
         {game_panel win}}
      {do
         {if
            {==
               {ui current_screen}
               song_select_screen}
            {do
               ($song
                  {song_select_panel get focus_song})
               {if
                  $song
                  {setup_game
                     $song
                     boardwalk
                     ((kDifficultyExpert))}
                  {meta_performer trigger_song_completion 666 $num_stars}
                  {meta_performer reset_songs}
                  {song_select_panel update_song_list}}}}
         {if
            {==
               {ui current_screen}
               seldiff_screen}
            {do
               ($song
                  {meta_performer song})
               ($diff
                  {switch
                     {{seldiff_panel loaded_dir}
                        get
                        focus_component}
                     (easy.btn kDifficultyEasy)
                     (medium.btn kDifficultyMedium)
                     (expert.btn kDifficultyExpert)
                     kDifficultyExpert})
               {if
                  $song
                  {meta_performer set_difficulty $diff}
                  {meta_performer trigger_song_completion 666 $num_stars}
                  {meta_performer reset_songs}
                  {meta_performer set_song $song}
                  {seldiff_panel set_up}}}}
         {if
            {==
               {ui current_screen}
               seldiff_practice_screen}
            {do
               ($song
                  {meta_performer song})
               ($diff
                  {switch
                     {{seldiff_practice_panel loaded_dir}
                        get
                        focus_component}
                     (easy.btn kDifficultyEasy)
                     (medium.btn kDifficultyMedium)
                     (expert.btn kDifficultyExpert)
                     kDifficultyExpert})
               {if
                  $song
                  {meta_performer set_difficulty $diff}
                  {meta_performer
                     trigger_song_completion
                     {'*' 20 $num_stars}
                     0}
                  {meta_performer reset_songs}
                  {meta_performer set_song $song}
                  {seldiff_practice_panel set_up}}}}
         {if
            {==
               {ui current_screen}
               seldiff_multiplayer_player1_screen}
            {do
               ($song
                  {meta_performer song})
               {if
                  $song
                  {meta_performer trigger_song_completion 0 $num_stars}
                  {meta_performer reset_songs}
                  {meta_performer set_song $song}
                  {seldiff_multiplayer_panel set_up}}}}}}}
