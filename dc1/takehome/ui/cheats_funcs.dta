{func
   cheat_toggle_difficulty
   {do
      ($new_diff -1)
      {if
         {== $new_diff -1}
         {set
            $new_diff
            {'+'
               {meta_performer get_difficulty}
               1}}
         {set
            $new_diff
            {mod
               $new_diff
               {size
                  (DIFF_SYMBOLS)}}}
         {cheat_display
            show
            {sprint
               "New Difficulty Set: "
               {elem
                  (DIFF_SYMBOLS)
                  $new_diff}}}}
      {meta_performer set_difficulty $new_diff}}}
