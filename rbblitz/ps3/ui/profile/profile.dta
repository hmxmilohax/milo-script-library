{new
   UIPanel
   profile_panel
   (file
      "profile.milo")
   (enter
      {shell_sfx set_interceptor $this})
   (exit
      {shell_sfx clear_interceptor})
   (on_select_msg
      {shell_sfx play_select_short})
   (BUTTON_DOWN_MSG
      {switch
         $action
         (kAction_PageDown
            {data_mine_mgr
               add_event
               enter_store
               (("from_screen"
                     "profile"))}
            {transition_to store_menu_screen})
         (kAction_PageUp
            {transition_to song_select_screen})
         (kAction_Cancel
            {shell_sfx play_back}
            {ui goto_screen splash_screen})
         (kAction_Start
            {options_screen set back_screen profile_screen}
            {ui goto_screen options_screen})
         kDataUnhandled})}
{new
   UIScreen
   profile_screen
   (panels background_panel shell_music_panel menu_cache_panel profile_panel)
   (header_index 2)
   (focus profile_panel)
   (enter
      {background_panel
         jump_to_header
         [header_index]})}