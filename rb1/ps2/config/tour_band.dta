; ACTIVE ROCK MOMENTS are stored in this array, since they are grouped
; by vehicle, they have this format:
; TODDO ACTIVE ROCK MOMENTS syntax...
#define ACTIVE_MOMENT_DOUBLE_OR_NOTHING
(
   (tour_am_doubleornothingfourstar
      ; this moment gives us $ x 2 if we get >=4 stars, $0 if < 4
      (reward_per_star ($stars $event_reward)
         {if_else {>= $stars 4}
            {* $event_reward 2}
            0
         }
      )
   )   
)

#define ACTIVE_MOMENT_QUADRUPLE_OR_NOTHING
(
   (tour_am_quadrupleornothingfivestar
      ; this moment gives us $ x 4 if we get >=5 stars, $0 if < 5
      (reward_per_star ($stars $event_reward)
         {if_else {>= $stars 5}
            {* $event_reward 4}
            0
         }
      )
   )   
)

;; charity - no money, but double fame
#define ACTIVE_MOMENT_CHARITY
(
   (tour_am_charity
      (reward_per_star ($stars $event_reward)
         0
      )
      (buzz_change ($avg_stars $buzz_change)
         {if_else {> $buzz_change 0}
            {* $buzz_change 2}
            $buzz_change
         }
      )
   )
)

;; no fans, but triple money
#define ACTIVE_MOMENT_SELLOUT
(
   (tour_am_sellout
      (reward_per_star ($stars $event_reward)
         {* $event_reward 3}
      )
      (buzz_change ($avg_stars $buzz_change)
         {if_else {> $buzz_change 0}
            {* $buzz_change 0.3}
            $buzz_change
         }
      )
   )
)

;; get 4*, and you'll get double the fans - below, and you'll LOSE double the fans
#define ACTIVE_MOMENT_DOUBLE_OR_NOTHING_FANS
(
   (tour_am_double_or_nothing_fans
      (buzz_change ($avg_stars $buzz_change)
         {if_else {>= $avg_stars 4}
            {* $buzz_change 2}
            {if_else {> $buzz_change 0}
               {* $buzz_change -2}
               {* $buzz_change 2}
            }
         }
      )
   )
)

;; get 5*, and you'll get triple the fans - below, and you'll LOSE triple the fans
#define ACTIVE_MOMENT_TRIPLE_OR_NOTHING_FANS
(
   (tour_am_triple_or_nothing_fans
      (buzz_change ($avg_stars $buzz_change)
         {if_else {>= $avg_stars 5}
            {* $buzz_change 3}
            {if_else {> $buzz_change 0}
               {* $buzz_change -3}
               {* $buzz_change 3}
            }
         }
      )
   )
)
         
(moments
   (frequency 2 5)  ; min/max events between active moments

   ; these vehicles must be in order and match the items awarded in events
   (vehicles
      ; this first level is also used when bands don't have any vehicle
      (tour_vehicle_van
         ACTIVE_MOMENT_DOUBLE_OR_NOTHING
         ACTIVE_MOMENT_QUADRUPLE_OR_NOTHING
         ACTIVE_MOMENT_CHARITY
      )
      (tour_vehicle_bus
         ACTIVE_MOMENT_DOUBLE_OR_NOTHING
         ACTIVE_MOMENT_QUADRUPLE_OR_NOTHING
         ACTIVE_MOMENT_CHARITY
         ACTIVE_MOMENT_SELLOUT
         ACTIVE_MOMENT_DOUBLE_OR_NOTHING_FANS
      )
      (tour_vehicle_jet
         ACTIVE_MOMENT_DOUBLE_OR_NOTHING
         ACTIVE_MOMENT_QUADRUPLE_OR_NOTHING
         ACTIVE_MOMENT_CHARITY
         ACTIVE_MOMENT_SELLOUT
         ACTIVE_MOMENT_DOUBLE_OR_NOTHING_FANS
         ACTIVE_MOMENT_TRIPLE_OR_NOTHING_FANS
      )
   )
)

; HINTS: order of these determines order they are displayed on the gig guide
; (<hint_name>              ; must be unique
;    ONEOFF_HINT            ; optional, means only displayed once per band
;    (cond <int>)           ; evaluated with $hint.value, if TRUE, hint is shown
;    (screen <symbol>)      ; evaluated with $hint.value, localized string for
;                           ; hint pop-up message
; )
(hints
   (welcome
      ONEOFF_HINT
      (cond TRUE)
      (screen tour_band_welcome_screen)
   )
   
   (firstgig
      ONEOFF_HINT
      (cond {> {{tour performer} world_fame} 0})
      (screen tour_band_firstgig_screen)
   )
   
   (firstgig_checkmarks
      ONEOFF_HINT
      (cond {> {{tour performer} world_fame} 0})
      (screen tour_band_firstgig_checkmarks_screen)
   )
   
   (first_venue
      ONEOFF_HINT
      (cond {{tour performer} can_leave_home_venue})
      (screen tour_band_firstvenue_screen)
   )

   (new_van
      (cond {== {{tour performer} unlocked_item} tour_vehicle_van})
      (screen tour_band_newvan_screen)
      (advance {{tour performer} clear_unlocked_item})
   )
   
   (new_bus
      (cond {== {{tour performer} unlocked_item} tour_vehicle_bus})
      (screen tour_band_newbus_screen)
      (advance {{tour performer} clear_unlocked_item})
   )
   
   (new_jet
      (cond {== {{tour performer} unlocked_item} tour_vehicle_jet})
      (screen tour_band_newjet_screen)
      (advance {{tour performer} clear_unlocked_item})
   )

   (new_item
      (cond {!= {{tour performer} unlocked_item} ''})
      (screen tour_band_newitem_screen)
      (advance {{tour performer} clear_unlocked_item})
   )
   
   (van_available
      ONEOFF_HINT
      (cond {{tour performer} can_win_item tour_vehicle_van})
      (screen tour_band_canwinitem_screen)
   )
   
   (first_manager
      ONEOFF_HINT
      (cond {{tour performer} can_leave_hometown})
      (screen tour_band_firstmanager_screen)
   )
   
   (bus_available
      ONEOFF_HINT
      (cond {{tour performer} can_win_item tour_vehicle_bus})
      (screen tour_band_canwinitem_screen)
   )
   (jet_available
      ONEOFF_HINT
      (cond {{tour performer} can_win_item tour_vehicle_jet})
      (screen tour_band_canwinitem_screen)
   )
   (roadies_available
      ONEOFF_HINT
      (cond {{tour performer} can_win_item tour_staff_roadies})
      (screen tour_band_canwinitem_screen)
   )
   (promoter_available
      ONEOFF_HINT
      (cond {{tour performer} can_win_item tour_staff_promoter})
      (screen tour_band_canwinitem_screen)
   )
   (security_available
      ONEOFF_HINT
      (cond {{tour performer} can_win_item tour_staff_security})
      (screen tour_band_canwinitem_screen)
   )
   (tourmanager_available
      ONEOFF_HINT
      (cond {{tour performer} can_win_item tour_staff_tourmanager})
      (screen tour_band_canwinitem_screen)
   )
   (hofcomplete_available
      ONEOFF_HINT
      (cond {{tour performer} can_win_item tour_halloffame_complete})
      (screen tour_band_canwinitem_screen)
   )
   (bwtcomplete_available
      ONEOFF_HINT
      (cond {{tour performer} can_win_item tour_bwt_complete})
      (screen tour_band_canwinitem_screen)
   )
   
   (first_fans_lost
      ONEOFF_HINT
      (cond {{tour performer} have_lost_fans})   
      (screen tour_band_fanslost_screen)
   )
   (first_buzz_cap  ; when we've hit a non-expert cap
      ONEOFF_HINT
      (cond {== {{tour performer} buzz_state} kTourBuzzCap})
      (screen tour_band_buzzcap_screen)
   )
   (fist_buzz_maxcap ; when we've hit the expert cap
      ONEOFF_HINT
      (cond {== {{tour performer} buzz_state} kTourBuzzMaxCap})
      (screen tour_band_buzzmaxcap_screen)
   )
)

; EVENTS: All tier tour events are in this file, events for towns are in run/tour
;
; Event Format:
;
; (<event_name> ; looked up in locale.dta, along with "<event_name>_desc"
;   (songs 
;      <song>
;      (<part> <rank>)
;      random
;      any
;      ...
;   )
;   (reward_per_star <int>) ; reward per player per star
;   (fame |(world <int>)| |(town <int>)|) ; world/town fame required (optional)
;   (buzz <int>) ; buzz required to play (optional)
;   (item <sym>)            ; required band item (optional)
;   (award_item <sym>)      ; item awarded when event is won (optional)
;   (display_song <TRUE|FALSE>) ; use first song as event name (default=FALSE)
;                               ; first song can NOT be a random/any song
;   (outfit                     ; outfits awarded (optional)
;     (guitar
;        (male   <male_category>   <male_outfit>)
;        (female <female_category> <female_outfit>)
;     )
;     (drum
;        (male   <male_category>   <male_outfit>)
;        (female <female_category> <female_outfit>)
;     )
;     (vocals
;        (male   <male_category>   <male_outfit>)
;        (female <female_category> <female_outfit>);         
;     )
;   )
;   (sellout <bool>) ; does this event feature rockin' advertisements?
;
;   ; minimum difficulty this event can be played at (optional)
;   (min_difficulty <kDifficultyEasy|kDifficultyMedium|kDifficultyHard|kDifficultyExpert>)
; )
;
; Song Format:
; <song>: explicit name of a song
;
; (<part> <rank>): alias for song ranked within particular part
;    <part> = < drum | guitar | vocals | bass | leader | band >
;       leader = song ranked by leader's instrument (guitar, drums, or vocals)
;       band = song ranked within entire band's part
;    <rank> = song ranking (1 = lowest)
;    
; any: user selects one song from all available songs
; random: one random song from all available songs (resolved last) 

#define BWT_BUZZ_EASYBLOCK ;; have to play above easy to pass
(
   (buzz 180)
   (min_difficulty kDifficultyMedium)
)

#define BWT_BUZZ_EASYBLOCKMINORPLUS ;; have to play a good chunk into medium to pass
(
   (buzz 210)
   (min_difficulty kDifficultyMedium)
)


#define BWT_BUZZ_EASYBLOCKPLUS ;; have to play a good chunk into medium to pass
(
   (buzz 250)
   (min_difficulty kDifficultyMedium)
)

#define BWT_BUZZ_EASYBLOCKMEGAPLUS ;; have to play a good chunk into medium to pass
(
   (buzz 325)
   (min_difficulty kDifficultyMedium)
)


#define BWT_BUZZ_MEDIUMBLOCK  ;; have to play above medium to pass
(
   (buzz 355)
   (min_difficulty kDifficultyHard)
)

#define BWT_BUZZ_MEDIUMBLOCKMINORPLUS ;; have to play a good chunk into hard to pass
(
   (buzz 400)
