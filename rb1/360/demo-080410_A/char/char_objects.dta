#define DEFORM_COMMON
((deform
      {if
         {$this exists deform_clips}
         {do
            ($clip
               {deform_clips find deform})
            {start_char_utl_deform}
            {char_utl_deform
               $this
               $clip
               ((0 1))
               FALSE}
            {$this
               iterate
               CharIKScale
               $s
               {$s capture_before}}
            {char_utl_deform
               $this
               $clip
               ((0
                     {$this deform_frame 0})
                  (1
                     {$this deform_frame 1})
                  (2
                     {$this deform_frame 2})
                  (3
                     {$this deform_frame 3})
                  (4
                     {$this deform_frame 4}))
               TRUE}
            {end_char_utl_deform}
            {$this
               iterate
               CharIKScale
               $s
               {$s capture_after}}
            {$this
               iterate
               CharIKHand
               $s
               {$s measure_lengths}}
            {$this
               iterate
               CharForeTwist
               $s
               {$s measure_length_scale}}
            {$this
               iterate
               CharCollide
               $c
               {$c compute_radius}}}}))
