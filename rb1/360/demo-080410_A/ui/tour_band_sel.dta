{new
   UIScreen
   tour_band_lineup_demote_confirm_screen
   (panels dialog_panel)
   (focus dialog_panel)
   (enter
      {do
         ($rowArray
            {tour_band_lineup_panel
               get_row
               {tour_band_lineup_menu_panel get row_num}})
         ($char_name
            {if_else
               {==
                  {elem $rowArray 3}
                  kMetaLineupSubOnly}
               {elem $rowArray 4}
               {elem $rowArray 2}})
         {dialog_panel
            set_yesno
            {sprintf
               {localize tour_band_lineup_confirm_demote_fmt}
               $char_name}
            no.btn}
         {dialog_panel set limit session}})
   (SELECT_MSG
      {switch
         $component
         (yes.btn
            {tour_band_lineup_panel
               demote_member_to_sub
               {tour_band_lineup_menu_panel get row_num}}
            {ui pop_screen tour_band_lineup_screen})
         (no.btn
            {ui pop_screen})})}
{new
   UIScreen
   tour_band_lineup_demote_missing_confirm_screen
   (panels dialog_panel)
   (focus dialog_panel)
   (enter
      {do
         ($rowArray
            {tour_band_lineup_panel
               get_row
               {tour_band_lineup_menu_panel get row_num}})
         ($char_name
            {if_else
               {==
                  {elem $rowArray 3}
                  kMetaLineupSubOnly}
               {elem $rowArray 4}
               {elem $rowArray 2}})
         {dialog_panel
            set_yesno
            {sprintf
               {if_else
                  {tour_band_lineup_panel
                     is_signed_in_profile
                     {tour_band_lineup_menu_panel get row_num}}
                  {localize tour_band_lineup_confirm_demote_fmt}
                  {localize tour_band_lineup_confirm_demote_missing_fmt}}
               $char_name}
            no.btn}
         {dialog_panel set limit session}})
   (SELECT_MSG
      {switch
         $component
         (yes.btn
            {tour_band_lineup_panel
               demote_signed_in_profile
               {tour_band_lineup_menu_panel get row_num}}
            {tour_band_lineup_panel
               demote_offline_member
               {tour_band_lineup_menu_panel get row_num}}
            {ui pop_screen tour_band_lineup_screen})
         (no.btn
            {ui pop_screen})})}
