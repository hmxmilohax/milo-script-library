{new
   UIPanel
   tour_band_sel_panel
   (file
      "tour_band_sel.milo")
   (focus band.lst)
   (band_provider 0)
   (enter
      {session clear}
      {tour clear_performer}
      {set
         [band_provider]
         {get_band_provider}}
      {band.lst
         set_provider
         [band_provider]}
      {session add_sink $this signin_changed}
      {memcardmgr add_sink $this save_load_all_complete_msg}
      {profile_mgr add_sink $this profile_activated_msg})
   (TRANSITION_COMPLETE_MSG
      {autoload})
   (exit
      {session remove_sink $this signin_changed}
      {memcardmgr remove_sink $this save_load_all_complete_msg}
      {profile_mgr remove_sink $this profile_activated_msg})
   (BUTTON_DOWN_MSG
      {switch
         $button
         (kPad_R1
            {if
               {!
                  {platform_mgr is_player_signed_in $player_num}}
               {platform_mgr signin 1}})
         kDataUnhandled})
   (SELECT_MSG
      {switch
         $component
         (band.lst
            {do
               ($pos
                  {band.lst selected_pos})
               {cond
                  ({[band_provider]
                        is_pos_create_band
                        $pos}
                     {tour
                        set_performer
                        {tour create_new_band}}
                     {tour set_bwtmode kMetaTour_StartNew}
                     {do
                        ($p_profile
                           {profile_mgr get_mapped_profile_from_player $player_num})
                        {if
                           {&&
                              $p_profile
                              {$p_profile is_first_band_create}
                              {gamemode get is_bwt}}
                           {first_band_create_screen set my_player_num $player_num}
                           {matchmaking_band_screen set needs_first_band_create TRUE}}}
                     {ui
                        goto_screen
                        {gamemode get matchmaking_screen}})
                  ({[band_provider]
                        is_pos_join_band
                        $pos}
                     {tour set_bwtmode kMetaTour_UnknownRemote}
                     {ui
                        goto_screen
                        {gamemode get matchmaking_screen}})
                  (TRUE
                     {if_else
                        {[band_provider]
                           is_band_local
                           $pos}
                        {do
                           {tour
                              set_performer
                              {[band_provider]
                                 get_band
                                 $pos}}
                           {tour set_bwtmode kMetaTour_KnownLocal}}
                        {do
                           {[band_provider]
                              select_remote_band
                              $pos}
                           {tour set_bwtmode kMetaTour_KnownRemote}}}
                     {ui
                        goto_screen
                        {gamemode get matchmaking_screen}})}})})
   (JOYPAD_CONNECT_MSG
      {band.lst
         set_provider
         [band_provider]})
   (signin_changed
      {autoload}
      {band.lst
         set_provider
         [band_provider]})
   (save_load_all_complete_msg
      {band.lst
         set_provider
         [band_provider]})
   (profile_activated_msg
      ($pad_num)
      {autoload}
      {band.lst
         set_provider
         [band_provider]})}
{new
   UIScreen
   tour_band_sel_screen
   (panels meta practice_space_panel tour_band_sel_panel helpbar fx)
   (helpbar
      (helpbar_back helpbar_selband))
   (focus tour_band_sel_panel)
   (back main_multimode_screen)
   (enter
      {presence_mgr set_assembling_band}
      {practice_space_panel
         select_cam
         "coop_enter"}
      {char_cache cache_update})
   (exit
      {presence_mgr set_prepare_to_rock})}
{new
   BandLineupPanel
   tour_band_lineup_panel
   (file
      "tour_band_lineup.milo")
   (focus player00.btn)
   (leader_row -1)
   (force_exit TRUE)
   LIMIT_TO_SESSION
   (enter
      {practice_space_panel
         select_cam
         "coop_enter"}
      {band_name.lbl
         set_localized
         {{tour performer}
            band_name}}
      {$this update})
   (update
      {$this build_resolution_list}
      {foreach
         $i
         (0 1 2 3)
         {do
            ($rowArray
               {$this get_row $i})
            ($is_visible TRUE)
            {{sprintf
                  "leader%02d.lbl"
                  $i}
               set_showing
               {elem $rowArray 0}}
            {printf
               "%i isLeader %i\n"
               $i
               {elem $rowArray 0}}
            {if
               {elem $rowArray 0}
               {set
                  [leader_row]
                  $i}}
            {{sprintf
                  "instrument%02d.lbl"
                  $i}
               set_localized
               {cnv_instrumenttoicon
                  {elem $rowArray 1}}}
            {{sprintf
                  "original%02d.lbl"
                  $i}
               set_localized
               {elem $rowArray 2}}
            {switch
               {elem $rowArray 3}
               (kMetaLineupEmpty
                  {set $is_visible FALSE})
               (kMetaLineupPlaying
                  {set $is_visible TRUE}
                  {{sprintf
                        "status%02d.lbl"
                        $i}
                     set
                     text_token
                     tour_band_lineup_status_playing})
               (kMetaLineupOffline
                  {set $is_visible TRUE}
                  {{sprintf
                        "status%02d.lbl"
                        $i}
                     set
                     text_token
                     tour_band_lineup_status_offline})
               (kMetaLineupSubOnly
                  {set $is_visible TRUE}
                  {{sprintf
                        "status%02d.lbl"
                        $i}
                     set_localized
                     {sprintf
                        {localize tour_band_lineup_status_sub_fmt}
                        {elem $rowArray 4}}})
               (kMetaLineupSubForOffline
                  {set $is_visible TRUE}
                  {{sprintf
                        "status%02d.lbl"
                        $i}
                     set_localized
                     {sprintf
                        {localize tour_band_lineup_status_sub_fmt}
                        {elem $rowArray 4}}})}
            {$this
               {if_else $is_visible enable disable}
               {sprintf
                  "player%02d.btn"
                  $i}}
            {{sprintf
                  "player%02d.grp"
                  $i}
               set_showing
               $is_visible}}})
   (SELECT_START_MSG
      {do
         ($row_num
            {switch
               $component
               (player00.btn 0)
               (player01.btn 1)
               (player02.btn 2)
               (player03.btn 3)})
         {do
            {tour_band_lineup_menu_panel set row_num $row_num}
            {ui goto_screen tour_band_lineup_menu_screen}
            kDataUnhandled}})}
{new
   UIScreen
   tour_band_lineup_screen
   (panels meta practice_space_panel tour_band_lineup_panel tour_band_lower3rd_panel helpbar)
   (helpbar
      (helpbar_back helpbar_continue))
   (focus tour_band_lineup_panel)
   (back tour_band_hub_screen)}
{new
   UIPanel
   tour_band_lineup_menu_panel
   (file
      "tour_band_lineup_menu.milo")
   (focus opt0.btn)
   (player_num -1)
   (row_num -1)
   LIMIT_TO_SESSION
   (setup
      ($player_num $row_num)
      {set
         [player_num]
         $player_num}
      {set
         [row_num]
         $row_num})
   (enter
      {do
         ($rowArray
            {tour_band_lineup_panel
               get_row
               [row_num]})
         ($is_leader
            {==
               {tour_band_lineup_panel get leader_row}
               [row_num]})
         ($is_promotable
            {==
               {elem $rowArray 3}
               kMetaLineupSubOnly})
         ($is_demotable
            {==
               {elem $rowArray 3}
               kMetaLineupPlaying})
         ($is_missing
            {'||'
               {==
                  {elem $rowArray 3}
                  kMetaLineupOffline}
               {==
                  {elem $rowArray 3}
                  kMetaLineupSubForOffline}})
         ($char_name
            {if_else
               {==
                  {elem $rowArray 3}
                  kMetaLineupSubOnly}
               {elem $rowArray 4}
               {elem $rowArray 2}})
         {$this enable opt1.btn}
         {opt1.btn set_showing TRUE}
         {cond
            ($is_leader
               {opt0.btn set text_token tour_band_lineup_view_char_profile}
               {$this disable opt1.btn}
               {opt1.btn set_showing FALSE})
            ($is_promotable
               {opt0.btn set text_token tour_band_lineup_promote}
               {opt1.btn set text_token tour_band_lineup_view_char_profile})
            ($is_demotable
               {opt0.btn set text_token tour_band_lineup_demote}
               {opt1.btn set text_token tour_band_lineup_view_char_profile})
            ($is_missing
               {opt0.btn set text_token tour_band_lineup_demote_missing}
               {$this disable opt1.btn}
               {opt1.btn set_showing FALSE})
            (TRUE
               {opt0.btn set text_token tour_band_lineup_view_char_profile}
               {$this disable opt1.btn}
               {opt1.btn set_showing FALSE})}
         {charname.lbl set_localized $char_name}})
   (SELECT_MSG
      {switch
         {$component get text_token}
         (tour_band_lineup_demote
            {ui push_screen tour_band_lineup_demote_confirm_screen})
         (tour_band_lineup_demote_missing
            {ui push_screen tour_band_lineup_demote_missing_confirm_screen})
         (tour_band_lineup_promote
            {if_else
               {profile_mgr
                  can_char_join_band
                  {{gamecfg
                        get_player_config
                        {tour_band_lineup_panel
                           get_player_num_from_row
                           [row_num]}}
                     get_char}}
               {ui push_screen tour_band_lineup_promote_confirm_screen}
               {ui push_screen tour_band_lineup_at_band_limit_screen}})
         (tour_band_lineup_view_char_profile
            {tour_char_profile_panel
               set
               profile_player_num
               {tour_band_lineup_panel
                  get_player_num_from_row
                  [row_num]}}
            {ui goto_screen tour_band_lineup_char_profile_screen})})}
