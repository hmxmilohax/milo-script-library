         {channels.lst selected_pos}}
      {if_else
         {!
            {gamemode get is_bwt}}
         {session_mgr find_players}
         {if_else
            {tour performer}
            {session_mgr find_bandmates}
            {session_mgr find_leader}}}
      {ui pop_screen})
   (BUTTON_DOWN_MSG
      {if_else
         {== $button kPad_Tri}
         {ui pop_screen}
         kDataUnhandled})}
{new
   UIScreen
   selchannel_screen
   (panels selchannel_panel)
   (focus selchannel_panel)}