                           {==
                              {gamecfg get leader_num}
                              -1}
                           {is_leader_local}}
                        {localize mm_title_choose_local_band}
                        {if_else
                           {&&
                              {tour performer}
                              {{tour performer}
                                 is_finalized}}
                           {{tour performer}
                              band_name}
                           {localize mm_title_join_band}}})
                  {localize mm_title_join_band}})
            (TRUE
               {if_else
                  {'||'
                     {==
                        {gamecfg get leader_num}
