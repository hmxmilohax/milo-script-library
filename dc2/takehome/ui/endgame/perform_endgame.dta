{new HamScreen
   campaign_perform_endgame_screen
   (panels meta perform_endgame_panel)
   (focus perform_endgame_panel)
   (enter
      {if_else {meta_performer just_finished_crew}
         {do
            ($crew {meta_performer get_crew})
            {cond
               ({== $crew crew05}
                  {perform_endgame_panel set
                     complete_screen
                     campaign_perform_skipcomplete_goto_finale_screen
                  }
               )
               {perform_endgame_panel set
                  complete_screen
                  campaign_perform_skipcomplete_screen
               }
            }
         }
         {if_else {>= {game_panel get num_stars} 4}
            {perform_endgame_panel set
               complete_screen
               campaign_perform_skipcomplete_screen
            }
            {perform_endgame_panel set
               complete_screen
               campaign_perform_complete_screen
            }
         }
      }
   )
}
