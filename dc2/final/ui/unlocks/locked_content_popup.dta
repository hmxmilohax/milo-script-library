{new
   LockedContentPanel
   locked_content_panel
   (file
      "locked_content_popup.milo")
   (show_hand_help FALSE)
   (focus right_hand.hnl)
   (back_token back)
   (enter)
   (exit)
   (NAV_SELECT_MSG
      {switch
         $component
         (right_hand.hnl
            {switch
               $name
               (locked_content_ok
                  {{ui current_screen}
                     dismiss})
               kDataUnhandled})
         (left_hand.hnl
            {switch
               $name
               (back
                  {{ui current_screen}
                     dismiss})
               kDataUnhandled})
         kDataUnhandled})
   (BUTTON_DOWN_MSG
      {switch
         $action
         (kAction_Cancel
            {{ui current_screen}
               dismiss})
         kDataUnhandled})}
{new
   HamScreen
   locked_content_screen
   (panels locked_content_panel)
   (focus locked_content_panel)
   (dismiss
      {run_flow left_select.flow}
      {ui pop_screen})}