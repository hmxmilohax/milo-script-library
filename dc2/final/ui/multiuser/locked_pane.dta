{new Object
   locked_pane
   (back_pane_s0 '')
   (back_pane_s1 '')
   (enter
      ($side)
      {multiuser_panel set_provider $side locked.hnp}
      {multiuser_panel set_list_dir $side label_checkbox}
   )
   (set_back_pane
      ($side $pane)
      {switch $side
         (0 {$this set back_pane_s0 $pane})
         (1 {$this set back_pane_s1 $pane})
      }
   )
   (get_back_pane
      ($side)
      {switch $side
         (0 [back_pane_s0])
         (1 [back_pane_s1])
      }
   )
   (set_difficulty
      ($side $song $difficulty)
      {do
         ($info {multiuser_panel find locked_info_p1})
         ($difficulty_format {sprintf "award_%s_instruction" $difficulty})
         {if {!= $side 0}
            {set $info {multiuser_panel find locked_info_p2}}
         }
         {{$info find content.lbl} set text_token $difficulty}
         {if_else {gamemode in_mode playlist_perform}
            {do
               {set $difficulty_format
                  {sprintf "award_%s_playlist_instruction" $difficulty}
               }
               {{$info find song.lbl} set text_token ''}
               {{$info find stars.std} set showing FALSE}
               {{$info find instructions.lbl} set text_token $difficulty_format}
            }
            {do
               {{$info find song.lbl} set_song_name $song}
               {{$info find stars.std} set_song_with_diff_always
                  {song_mgr get_song_id $song}
                  {- {sym_to_difficulty $difficulty} 1}
               }
               {{$info find stars.std} set showing TRUE}
               {{$info find instructions.lbl} set_token_fmt $difficulty_format 3}
            }
         }
      }
   )
   (set_asset
      ($side $asset)
      {do
         ($info {multiuser_panel find locked_info_p1})
         {if {!= $side 0}
            {set $info {multiuser_panel find locked_info_p2}}
         }
         {{$info find content.lbl} set text_token $asset}
         {{$info find instructions.lbl} set
            text_token
            {sprintf "award_%s_instruction" $asset}
         }
         {{$info find song.lbl} set text_token ''}
         {{$info find stars.std} set showing FALSE}
      }
   )
   (exit ($side))
   (refresh
      ($side)
      {multiuser_panel enable_list $side}
   )
   (highlight ($side $name $index))
   (select
      ($side $name $index)
      {switch $name
         (back {multiuser_panel set_pending_pane $side {$this get_back_pane $side}})
         (ok {multiuser_panel set_pending_pane $side {$this get_back_pane $side}})
      }
   )
}