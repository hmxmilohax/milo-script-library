      {if_else
         $mp.single_bit
         {if_else
            {==
               [streak_count]
               0}
            {do
               {set
                  [streak_count]
                  1}
               {set
                  [streak_start]
                  $mp.start}}
            {if_else
               {< $mp.prev_start 0.75}
               {do
                  {'++'
                     [streak_count]}
                  {set
                     [last_start]
                     $mp.start}}
               {do
                  {if
                     {>
                        [streak_count]
                        3}
                     {set $orig_start $mp.start}
                     {set
                        $mp.start
                        [streak_start]}
                     {set
                        $mp.end
                        [streak_start]}
                     {$this add_message lookat_in}
                     {set
                        $mp.start
                        [last_start]}
                     {set
                        $mp.end
                        [last_start]}
                     {$this add_message lookat_out}}
                  {set
                     [streak_count]
                     0}}}}
         {do
            {set
               [streak_count]
               0}}})}
