         (range 0 255)
         (slider)
         (alias
            "value")
         (help
            "the value to paint into the seleced channel")
         (hide
            {'||'
               {!=
                  [mvp_channel]
                  kChannelColor}
               {==
                  [mvp_color_mode]
                  kModeRGB}}))
      (mvp_colorfill
         script
         (script
            {$this mvp_color_fill})
         (no_test)
