   (hide_helpbar TRUE)
   (allow_doubleuser_swipe TRUE)
   (enter
      {right_hand.hnl set showing TRUE}
      {right_hand.hnl enable_selection}
      {if_else
         {!
            {partymode_mgr is_team_signed_in 1}}
         {if_else
            {'||'
               {!
