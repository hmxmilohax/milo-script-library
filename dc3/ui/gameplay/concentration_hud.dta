#define CNCNTRTN_STATE_LOADING
(1)
#define CNCNTRTN_STATE_DANCE
(2)
#define CNCNTRTN_STATE_SCORE
(3)
#define CNCNTRTN_STATE_GAME_OVER
(4)
#define CNCNTRTN_MOVE_DETECT_THRESHHOLD
(0.75)
(concentration_scriptobj
   #include performance.dta
   (do_detection FALSE)
   (row_count 3)
   (col_count 4)
   (score_curve
      "")
   (col_offset 200)
