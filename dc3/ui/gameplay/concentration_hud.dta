                        {set
                           $dancer
                           {$card2 get dancer}}}
                     {if_else
                        {>
                           {$card get flip_beat}
                           {$card2 get flip_beat}}
                        {set
                           $dancer
                           {$card get dancer}}
                        {set
                           $dancer
                           {$card2 get dancer}}}}
                  {$this score_set $card $card2 $dancer}}}}})
   (score_set
      ($card $card2 $dancer)
      {do
         ($score 1000)
         ($result)
         {$card
            set_score
            $dancer
            $score
            {$card get result}}
         {$card show_score}
         {$card2
            set_score
            $dancer
            $score
            {$card2 get result}}
         {$card2 show_score}
         {set_elem
            [player_scores]
            $dancer
            {'+'
               $score
