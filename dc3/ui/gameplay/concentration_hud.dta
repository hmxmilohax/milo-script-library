            {set
               $p2_scoring
               {$this get_highest_scored_move 1}}
            {if
               {<
                  {elem $p2_scoring 1}
                  CNCNTRTN_MOVE_DETECT_THRESHHOLD}
               {if
                  {elem
                     [cheat_moves]
                     1}
                  {set_elem
                     $p2_scoring
                     0
                     {{elem
                           [cheat_moves]
                           1}
                        get
                        move}}
                  {set_elem $p2_scoring 1 CNCNTRTN_MOVE_DETECT_THRESHHOLD}}}
            {set_elem
               [cheat_moves]
               0
               ""}
            {set_elem
               [cheat_moves]
               1
               ""}
            {if
               {==
                  {elem $p1_scoring 0}
                  {elem $p2_scoring 0}}
               {if_else
                  {<=
                     {elem $p1_scoring 1}
                     {elem $p2_scoring 1}}
                  {set_elem $p1_scoring 1 0.0}
                  {set_elem $p2_scoring 1 0.0}}}
            {if
               {>=
                  {elem $p1_scoring 1}
                  CNCNTRTN_MOVE_DETECT_THRESHHOLD}
               {$this
                  score_move
                  {elem $p1_scoring 0}
                  0
                  {elem $p1_scoring 1}}}
            {if
               {>=
                  {elem $p2_scoring 1}
                  CNCNTRTN_MOVE_DETECT_THRESHHOLD}
               {$this
                  score_move
                  {elem $p2_scoring 0}
                  1
                  {elem $p2_scoring 1}}}}})
   (score_move
      ($ham_move $dancer $result)
      {$this
         log
         1
         ("score move"
