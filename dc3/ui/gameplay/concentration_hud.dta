            [card_set]
            {if
               {!
                  {$card get flipped}}
               {push_back $cards_unflipped $card}}}
         {set
            $card
            {random_elem $cards_unflipped}}
         $card})
   (cheat_move
      ($dancer)
      {if
         {==
            [play_state]
            CNCNTRTN_STATE_DANCE}
