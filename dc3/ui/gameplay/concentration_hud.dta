                  $move_variant
                  {$card get move_variant}}}}
         $move_variant})
   (get_card_from_move
      ($ham_move)
      {do
         ($card)
         ($move_card
            "")
         {set
            $move_card
            ""}
         {foreach
            $card
            [card_set]
            {if
               {==
                  $ham_move
                  {$card get move}}
               {set $move_card $card}}}
         $move_card})
   (get_highest_scored_move
      ($dancer)
      {do
         ($ham_move)
         ($result)
         ($normalized_result)
         ($raw_detection_val)
         ($card
            "")
         ($best_ham_move
            "")
         {set $result 0.0}
         {foreach
            $ham_move
            [ham_moves]
            {set
               $raw_detection_val
               {[move_dir]
                  last_detector_result
                  $dancer
                  $ham_move}}
            {set
