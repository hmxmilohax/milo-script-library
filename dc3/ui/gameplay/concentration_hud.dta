            $dancer
            ", move="
            {$ham_move display_name}
            ", result="
            $result)}
      {do
         ($card)
         {set
            $card
            {$this get_card_from_move $ham_move}}
         {$card set dancer $dancer}
         {$this card_flip_over $card}})
   (beat
      {'++'
         [state_beats]}
