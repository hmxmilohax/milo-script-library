                           update_feedback
                           {$this get_is_right_player $dancer}
                           $normalized_result}}}}}}})
   (hm2mv
      ($ham_move)
      {do
         ($move_variant
            "")
         ($card)
         {foreach
            $card
            [card_set]
            {if
               {==
                  $ham_move
