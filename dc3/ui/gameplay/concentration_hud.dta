            {$flow activate}
            {$this
               log
               2
               ("flow not found: "
                  $flow_name)}}})
   (poll
      {$this
         log
         0
         ("poll")}
      {if
         {&&
            [do_detection]
            {==
               [play_state]
               CNCNTRTN_STATE_DANCE}}
