      {if
         {&&
            [do_detection]
            {==
               [play_state]
               CNCNTRTN_STATE_DANCE}}
         {do
            ($p1_scoring
               ())
            ($p2_scoring
               ())
            ($card)
            {set
               $p1_scoring
               {$this get_highest_scored_move 0}}
