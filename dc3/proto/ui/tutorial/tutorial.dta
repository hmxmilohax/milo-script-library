#define TUTORIAL_PANEL_COMMON (
   (focus right_hand.hnl)
   (helpbar_allow_controller FALSE)
   (ready_disable_controller FALSE)
   (continue_token_displayed FALSE)
   (hide_helpbar FALSE)
   (back_token '')
   (tertiary_token skip_tutorial)
   (show_back_controller_icon FALSE)
   (enter
      {if [enable_tertiary_skip]
         {set $lefthand {helpbar find left_hand.hnl}}
         {$lefthand set_showing TRUE}
         {$lefthand enable_navigation}
         {$lefthand set nav_provider {helpbar find tertiary_only.hnp}}
      }
      {set [helpbar_allow_controller] TRUE}
      {$this set helpbar_confirm_label skip}
      {helpbar resync}
      {right_hand.hnl set_provider provider_skip.hnp}
      {right_hand.hnl set_showing TRUE}
      {set [continue_token_displayed] FALSE}
      {voice_input_panel activate_voice_context tutorial_screen}
   )
   (exit
      {set [ready_disable_controller] FALSE}
      {set [helpbar_allow_controller] FALSE}
   )
   (controller_mode_entered {right_hand.hnl set_showing FALSE})
   (controller_mode_exited {right_hand.hnl set_showing TRUE})
   (switch_button_labels
      {if_else {== [continue_token] tutorial_done}
         {right_hand.hnl set_provider provider_tutorial_done.hnp}
         {right_hand.hnl set_provider provider_continue.hnp}
      }
      {$this set helpbar_confirm_label [continue_token]}
      {helpbar resync}
   )
   (TRANSITION_COMPLETE_MSG
      {$this set helpbar_confirm_label skip}
      {right_hand.hnl set_provider provider_skip.hnp}
      {helpbar resync}
   )
   (NAV_SELECT_MSG
      {switch $component
         (right_hand.hnl
            {if_else {tut_nav.flow is_running}
               {do
                  {$this switch_button_labels}
                  {if {exists tut_nav_skip.flow}
                     {tut_nav_skip.flow activate}
                  }
               }
               {ui goto_screen {{ui current_screen} get next_screen}}
            }
         )
         (left_hand.hnl {ui goto_screen {{ui current_screen} get exit_screen}})
      }
   )
   #ifndef _SHIP
   (BUTTON_DOWN_MSG
      {switch $raw_button
         (kPad_Xbox_Y {ui goto_screen {{ui current_screen} get exit_screen}})
         kDataUnhandled
      }
   )
   #endif
   SELECT_RIGHT_LIST_TOKEN
   (poll
      SLIDE_RIGHT_LIST_ITEM
      {if {&& {tut_nav.flow is_running} {! [ready_disable_controller]}}
         {set [ready_disable_controller] TRUE}
      }
      {if
         {&&
            [ready_disable_controller]
            {! {tut_nav.flow is_running}}
            {! [continue_token_displayed]}
         }
         {set [continue_token_displayed] TRUE}
         {$this switch_button_labels}
      }
   )
   (on_voice_command
      ($command)
      {switch $command
         (continue {$this select_right_list_token continue})
         (skip_tutorial
            {if [enable_tertiary_skip]
               {helpbar select_left_list_item 0}
            }
         )
      }
   )
)
