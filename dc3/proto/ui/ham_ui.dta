(augmented_photo "ui/augmented_photo.milo")
#define COMMON_ERROR_HANDLERS (COMMON_DIALOG_PANEL_HANDLERS (helpbar ((confirm helpbar_select))))
(ui_event_mgr
   (dialog_events
      (error_message
         COMMON_ERROR_HANDLERS
         (init
            ($token)
            {voice_input_panel disable_currrent_voice_context}
            {$this set_ok $token FALSE}
            {if {exists event_dialog.grp}
               {event_dialog.grp set_showing TRUE}
            }
         )
         (NAV_SELECT_DONE_MSG
            {if {exists event_dialog.grp}
               {event_dialog.grp set_showing FALSE}
            }
            {voice_input_panel restore_currrent_voice_context}
            {ui_event_mgr dismiss_event}
         )
      )
      (saveload_dialog_event
         COMMON_ERROR_HANDLERS
         (init
            {do
               ($msg {saveload_mgr get_dialog_msg})
               ($opt1 {saveload_mgr get_dialog_opt1})
               ($opt2 {saveload_mgr get_dialog_opt2})
               ($focus_index {saveload_mgr get_dialog_focus_option})
               ($focus right_hand.hnl)
               {voice_input_panel disable_currrent_voice_context}
               {if {exists event_dialog.grp}
                  {event_dialog.grp set_showing TRUE}
               }
               {if_else {!= $opt2 ''}
                  {$this set_custom_ok_no $msg $opt1 $opt2 $focus_index FALSE}
                  {$this set_ok $msg FALSE}
               }
            }
         )
         SELECT_RIGHT_LIST_TOKEN
         (poll SLIDE_RIGHT_LIST_ITEM)
         (NAV_SELECT_MSG
            {if {== $component right_hand.hnl}
               {saveload_mgr handle_eventresponse_start {'+' 1 $index}}
            }
         )
         (NAV_SELECT_DONE_MSG
            {if {== $component right_hand.hnl}
               {if {exists event_dialog.grp}
                  {event_dialog.grp set_showing FALSE}
               }
               {voice_input_panel restore_currrent_voice_context}
               {ui_event_mgr dismiss_event}
               {saveload_mgr handle_eventresponse
                  {profile_mgr get_active_profile}
                  {'+' 1 $index}
               }
            }
         )
      )
   )
   (transition_events
      (leaderboard_error
         (next_screen meta_loading_leaderboard_error_screen)
         (destination_screen
            {meta_loading_leaderboard_error_screen get next_screen}
         )
         (init
            {ui_event_mgr trigger_event
               error_message
               (dummy init error_lost_leaderboards)
            }
         )
      )
      (storage_changed
         (next_screen meta_loading_uisink_default_screen)
         (destination_screen {meta_loading_uisink_default_screen get next_screen})
         (init
            {ui_event_mgr trigger_event
               error_message
               (dummy init error_storage_devices_changed)
            }
         )
      )
      (data_error
         (next_screen meta_loading_uisink_default_screen)
         (destination_screen {meta_loading_uisink_default_screen get next_screen})
         (init
            ($corrupt $info_str)
            {ui_event_mgr trigger_event
               error_message
               (dummy
                  init
                  {if_else $corrupt
                     (error_corrupt_song_data $info_str)
                     error_lost_song_data
                  }
               )
            }
         )
      )
      (sign_out
         (next_screen meta_loading_signout_screen)
         (destination_screen {meta_loading_signout_screen get next_screen})
         (init
            ($reason)
            {ui_event_mgr trigger_event
               error_message
               (dummy
                  init
                  {switch $reason
                     (kLocalUserSignedOut error_local_user_signed_out)
                     (kProfileOwnerSignedOut error_profile_owner_signed_out)
                  }
               )
            }
         )
      )
      (sign_in
         (next_screen meta_loading_signout_screen)
         (destination_screen {meta_loading_signout_screen get next_screen})
         (init
            ($reason)
            {ui_event_mgr trigger_event
               error_message
               (dummy init error_local_user_signed_out)
            }
         )
      )
      (disc_error
         (next_screen meta_loading_uisink_lost_connection_screen)
         (destination_screen
            {meta_loading_uisink_lost_connection_screen get next_screen}
         )
         (init {ui_event_mgr trigger_event error_message (dummy init error_disc)})
      )
      (store_user_change
         (next_screen meta_loading_signout_screen)
         (destination_screen {meta_loading_signout_screen get next_screen})
         (init
            {ui_event_mgr trigger_event
               error_message
               (dummy init error_store_user_change)
            }
         )
      )
      (store_load_failed
         (next_screen meta_loading_uisink_default_screen)
         (destination_screen {meta_loading_uisink_default_screen get next_screen})
         (init
            ($reason)
            {ui_event_mgr trigger_event
               error_message
               (dummy
                  init
                  {switch $reason
                     (kStoreErrorNoContent store_error_no_content)
                     (kStoreErrorLiveServer store_error_live_server)
                     (kStoreErrorStoreServer store_error_store_server)
                     (kStoreErrorSignedOut error_local_user_signed_out)
                     (kStoreErrorDroppedOut error_local_user_signed_out)
                     (kStoreErrorNoMetadata store_error_no_metadata)
                     (kStoreErrorNoEthernetCable error_ethernet_unplugged)
                  }
               )
            }
         )
      )
   )
)