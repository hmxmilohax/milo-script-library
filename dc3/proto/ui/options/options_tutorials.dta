{new HamPanel
   options_tutorials_panel
   (file "options_tutorials.milo")
   (focus right_hand.hnl)
   (back_token back)
   (enter
      {hamprovider set ui_nav_mode shell}
      {voice_input_panel activate_voice_context options_tutorials_screen}
      {provider.hnp set_hidden
         voice_control
         {! {shell_input voice_control_enabled}}
      }
      {provider.hnp set_enabled
         voice_control
         {shell_input voice_control_enabled}
      }
   )
   (exit)
   SELECT_RIGHT_LIST_TOKEN
   (poll SLIDE_RIGHT_LIST_ITEM)
   (NAV_SELECT_MSG
      {switch $name
         (perform {enter_tutorial tutorial_perform_it_screen_1 TRUE})
         (practice {enter_tutorial tutorial_practice_screen_1 TRUE})
         (dance_battle {enter_tutorial tutorial_battle_screen_1 TRUE})
         (playlists {enter_tutorial tutorial_playlist_screen_1 TRUE})
         (crew_showdown {enter_tutorial tutorial_crew_showdown_screen_1 TRUE})
         (party_mode {enter_tutorial tutorial_party_mode_screen_1 TRUE})
         (voice_control {enter_tutorial tutorial_voice_control_screen_1 TRUE})
         (challenges {enter_tutorial tutorial_challenge_mode_screen_1 TRUE})
         (fitness_mode {enter_tutorial tutorial_fitness_screen_1 TRUE})
         (rhythm_battle {enter_tutorial tutorial_rhythm_battle_screen_1 TRUE})
         (campaign_master_quest
            {enter_tutorial tutorial_master_quest_screen_1 TRUE}
         )
         (strike_a_pose {enter_tutorial tutorial_strike_a_pose_screen_1 TRUE})
         (bustamove {enter_tutorial tutorial_make_your_move_screen_1 TRUE})
         (back {$this on_back})
      }
   )
   (BUTTON_DOWN_MSG
      {switch $action
         (
            (kAction_Cancel)
            {$this on_back}
         )
         kDataUnhandled
      }
   )
   (on_back {ui goto_screen {options_tutorials_screen get back}})
}
{new HamScreen
   options_tutorials_screen
   (panels meta background_panel options_tutorials_panel)
   (focus options_tutorials_panel)
   (back game_guide_screen)
   (on_voice_command
      ($command)
      {switch $command
         BACRO
         {options_tutorials_panel select_right_list_token $command}
      }
   )
}