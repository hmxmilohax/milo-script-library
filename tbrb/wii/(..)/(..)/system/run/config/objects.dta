                     {{$this dir}
                        supported_events}))
               (post_sync
                  {$this restart})
               (refresh current))
            (current
               symbol
               (read_only TRUE))
            (fire
               script
               (script
                  {$this fire})
               (refresh current))
            (skip
               script
               (script
                  {$this skip})
               (refresh current))
            (reset
               script
               (script
                  {$this reset})
               (refresh current)))
         (current
            {if_else
               {size
                  [events]}
               {elem
                  [events]
                  [current_index]}
               ''})
         (current_index 0)
         (events
            ())
         (fire
            {handle
               ({$this dir}
                  {elem
                     [events]
                     [current_index]})}
            {$this skip})
         (skip
            {'++'
               [current_index]}
            {if
               {>=
                  [current_index]
                  {size
                     [events]}}
               {set
                  [current_index]
                  0}})
         (reset
