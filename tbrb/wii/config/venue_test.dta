(init
   {set $composite_character_demand_load TRUE}
   {set $composite_character_swap_gender TRUE}
   {func
      init_venue
      ($dir $is_multiplayer)}
   {func
      get_venue_column_value
      ($dir $column)}
   {func
      get_shot_column_value
      ($shot $column)}
   {set
      $keyboard_configurations
      {'|' kP9KeyboardJohn kP9KeyboardPaul kP9KeyboardGeorge}}
   {set
      $regular_configurations
      {&
         {'~'
            {'|' $keyboard_configurations kP9Sitar kP9RingoSolo}}
         kP9AllConfigurations}}
   {func
      select_configuration
      ($flags $piano $waypoint)
      {set
         $configuration
         {&
            $flags
            {if_else $piano $keyboard_configurations $regular_configurations}}}
      {if
         {! $configuration}
         {set
            $configuration
            {& $flags kP9AllConfigurations}}}
      {$waypoint set configuration $configuration}
      {$waypoint view_configuration}
      {cond
         ({& $configuration $regular_configurations}
            "none")
         ({& $configuration kP9KeyboardJohn}
            "john")
         ({& $configuration kP9KeyboardPaul}
            "paul")
         ({& $configuration kP9KeyboardGeorge}
            "george")
         (default
            "none")}})
(set_camera
   ($shot))
