      (category
         (mixer
            (outfits P9_OUTFIT_MIXERS))))
   (extras_clips
      (category
         (venues
            (outfits
               (none)
               (budokan)
               (cavern)
               (dreamscape)
               (rooftop)
               (shea)
               (sullivan)))))
   #include extras.dta)