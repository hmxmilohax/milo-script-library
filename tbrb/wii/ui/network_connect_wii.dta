{new BandScreen
   changing_username_net_error_screen
   (panels dialog_panel)
   (focus dialog_panel)
   (block_event ($event) TRUE)
   (enter
      {do
         {set $server_connect_error error_lost_server}
         {set $result {platform_mgr last_net_result}}
         {set $dns_result {platform_mgr last_dns_error}}
         {set $dwcResultServer {server get_dwc_error}}
         {set $last_result {platform_mgr last_dwc_error}}
         {do
            {if_else {> $result 50000}
               {set $server_connect_error {platform_mgr get_dwc_error_string $result}}
               {if_else {== $dns_result 4}
                  {set $server_connect_error {platform_mgr get_dwc_error_string $dns_result}}
                  {if_else {> $dwcResultServer 20000}
                     {set $server_connect_error
                        {platform_mgr get_dwc_error_string $dwcResultServer}
                     }
                     {if {> $last_result 20000}
                        {set $server_connect_error
                           {platform_mgr get_dwc_error_string $last_result}
                        }
                     }
                  }
               }
            }
         }
         {print
            "CCS DEBUG: changing_username_net_error_screen"
            "\n -dwcResultServer: "
            $dwcResultServer
            "\n -last_result: "
            $last_result
            "\n"
         }
         {dialog_panel set_ok $server_connect_error}
      }
   )
   (SELECT_MSG
      {do
         {ui pop_screen main_popup_swapper}
      }
   )
}
