{new UIPanel
   deleting_friends_panel
   (file "server_message.milo")
   (focus message.lbl)
   (destination '')
   (username "")
   (enter
      {message.lbl set text_token connecting_server}
      {invitations add_sink $this (delete_op_complete)}
      {invitations clear_all_friends}
   )
   (delete_op_complete
      ($success)
      {print "CCS DEBUG: delete_op_complete " $success "\n"}
      {if $success
         {do
            {leaderboards set_offline FALSE}
            {print "CCS DEBUG: Now storing username: " [username] "\n"}
            {profile_mgr set_username [username]}
            {server clear_new_username}
            {finish_network_goto_with_autosave [destination] TRUE}
         }
      }
   )
   (exit {invitations remove_sink $this delete_op_complete})
}
