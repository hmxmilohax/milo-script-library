{new UIPanel
   server_connect_panel
   (file "server_message.milo")
   (destination '')
   (strict '')
   (username "")
   (change_username FALSE)
   (wait_for_naming FALSE)
   (block_event ($event) TRUE)
   (enter {message.lbl set text_token connecting_server})
   (TRANSITION_COMPLETE_MSG
      {platform_mgr add_sink $this (connection_status_changed)}
      {server add_sink $this (server_status_changed)}
      {if {! [change_username]}
         {server add_sink $this (naming_result_msg)}
      }
      {set $waiting_for_server TRUE}
   )
   (exit
      {if {! [change_username]}
         {server remove_sink $this naming_result_msgs}
      }
      {server remove_sink $this server_status_changed}
      {platform_mgr remove_sink $this connection_status_changed}
   )
   (naming_result_msg
      ($result)
      {print
         "CCS DEBUG: naming_result_msg: "
         $result
         " [wait_for_naming]: "
         [wait_for_naming]
         "\n"
      }
      {if {! [change_username]}
         {print "CCS DEBUG: Rock Central naming returns: " $result "\n"}
         {switch $result
            (NAMING_RESULT_NONE {set [wait_for_naming] FALSE})
            (NAMING_RESULT_DUPLICATE
               {rockcentral_username_error_screen set username [username]}
               {rockcentral_username_error_screen set
                  quazal_error
                  NAMING_RESULT_DUPLICATE
               }
               {ui goto_screen rockcentral_username_error_screen}
            )
            (NAMING_RESULT_PROFANE
               {rockcentral_username_error_screen set username [username]}
               {rockcentral_username_error_screen set quazal_error NAMING_RESULT_PROFANE}
               {ui goto_screen rockcentral_username_error_screen}
            )
            (NAMING_RESULT_INVALID
               {rockcentral_username_error_screen set username ""}
               {rockcentral_username_error_screen set quazal_error NAMING_RESULT_INVALID}
               {ui goto_screen rockcentral_username_error_screen}
            )
            (NAMING_RESULT_UNKNOWN
               {rockcentral_username_error_screen set quazal_error NAMING_RESULT_UNKNOWN}
               {ui goto_screen rockcentral_username_error_screen}
            )
         }
      }
   )
   (server_status_changed
      ($connected)
      {if {&& {! $connected} $waiting_for_server}
         {set [wait_for_naming] FALSE}
         {if_else {! [strict]}
            {do
               {rockcentral_error_screen set destination [destination]}
               {ui goto_screen rockcentral_error_screen}
            }
            {$this cancel_goto}
         }
      }
   )
   (connection_status_changed
      ($connected)
      {if {&& {! $connected} $waiting_for_server}
         {unless [wait_for_naming]
            {if_else {! [strict]}
               {do
                  {rockcentral_error_screen set destination [destination]}
                  {ui goto_screen rockcentral_error_screen}
               }
               {$this cancel_goto}
            }
         }
      }
   )
   (cancel_goto
      {unless [wait_for_naming]
         {set $waiting_for_server FALSE}
         {cancel_network_goto}
      }
   )
   (poll
      {if $waiting_for_server
         {switch {rock_central state}
            (kRCOnline
               {unless [wait_for_naming]
                  {set $waiting_for_server FALSE}
                  {leaderboards set_offline FALSE}
                  {if_else {server check_for_new_username}
                     {do
                        {deleting_friends_panel set destination [destination]}
                        {deleting_friends_panel set username [username]}
                        {ui goto_screen deleting_friends_screen}
                     }
                     {if_else {== [destination] changing_username_screen}
                        {finish_network_goto [destination] FALSE}
                        {finish_network_goto_with_autosave [destination] TRUE}
                     }
                  }
               }
            )
         }
      }
   )
}
