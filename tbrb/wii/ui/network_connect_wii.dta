   (helpbar ((min_height 0)))
}
{new UIPanel
   network_start_continue_panel
   (file "server_message.milo")
   (destination '')
   (strict FALSE)
   (wait_for_dwc_profane FALSE)
   (send_net_start FALSE)
   (connected FALSE)
   (change_username FALSE)
   (block_event ($event) TRUE)
   (enter
      {message.lbl set text_token wii_dwc_connecting}
      {platform_mgr add_sink $this}
   )
   (TRANSITION_COMPLETE_MSG
      {if_else {! {platform_mgr is_connected}}
         {platform_mgr init_nintendo_connection}
         {$this net_start_utility_finished TRUE}
      }
   )
   (poll
      {if {&& {! [wait_for_dwc_profane]} [send_net_start]}
         {set [send_net_start] FALSE}
         {$this continue_server_connect}
      }
   )
   (dwc_profanity_result_msg
      ($success)
      {set [wait_for_dwc_profane] FALSE}
   )
   (net_start_utility_finished
      ($connected)
      {set [connected] $connected}
      {if {'||' {server check_for_new_username} {profile_mgr check_username}}
         {if {! [change_username]}
            {if [connected]
               {if_else {profile_mgr check_username}
                  {do
                     {platform_mgr check_profane {profile_mgr get_username}}
                     {set [wait_for_dwc_profane] TRUE}
                  }
                  {do
                     {platform_mgr check_profane {server get_username}}
                     {set [wait_for_dwc_profane] TRUE}
                  }
               }
            }
         }
      }
      {set [send_net_start] TRUE}
   )
   (continue_server_connect
      {platform_mgr remove_sink $this}
      {if_else [connected]
         {do
            {if_else {'||' {server check_for_new_username} {profile_mgr check_username}}
               {if_else {&& {! {server is_connected}} {gamemode get rock_central}}
                  {do
                     {platform_mgr set_connected [connected]}
                     {server_connect_panel set destination [destination]}
                     {server_connect_panel set strict [strict]}
                     {ui goto_screen server_connect_screen}
                  }
                  {do
                     {platform_mgr set_connected [connected]}
                     {if_else {== [destination] changing_username_screen}
                        {finish_network_goto [destination] FALSE}
                        {finish_network_goto [destination] TRUE}
                     }
                  }
               }
               {do
                  {if {! [strict]}
                     {network_warning_screen set destination [destination]}
                  }
                  {if_else {memcardmgr file_exists}
                     {do
                        {username_info_screen set destination [destination]}
                        {username_info_screen set strict [strict]}
                        {ui goto_screen username_info_screen}
                     }
                     {do
                        {network_save_error_screen set destination [destination]}
                        {network_save_error_screen set strict [strict]}
                        {ui goto_screen network_save_error_screen}
                     }
                  }
               }
            }
         }
         {if_else {== [destination] changing_username_screen}
            {ui goto_screen changing_username_net_error_screen}
            {do
               {platform_mgr set_connected FALSE}
               {wii_dwc_error_screen set strict [strict]}
               {wii_dwc_error_screen set destination [destination]}
               {ui goto_screen wii_dwc_error_screen}
            }
         }
      }
   )
   (exit
      {set [change_username] FALSE}
      {platform_mgr remove_sink $this}
   )
}
