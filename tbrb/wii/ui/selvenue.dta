{new
   UIPanel
   selvenue_panel
   (file
      "selvenue.milo")
   (focus venue.lst)
   (enter
      {venue.lst
         set_data
         (default_venue VENUES)}
      {$this update_venue_selection})
   (exit
      {do
         ($sel
            {venue.lst selected_sym})
         {switch
            $sel
            (default_venue
               {meta_performer set_venue_select_type kVenueSelectDefault})
            {meta_performer set_venue $sel}}})
   (game_changed
      {$this update_venue_selection})
   (update_venue_selection
      {switch
         {meta_performer get_venue_select_type}
         (kVenueSelectDefault
            {venue.lst set_selected default_venue})
         {venue.lst
            set_selected
            {meta_performer get_venue}}})
   (SELECT_MSG
      {ui sync_screen song_select_screen 0})}
{new
   BandScreen
   selvenue_screen
   (panels meta background_panel postsong_sfx_panel selvenue_panel song_sync_panel)
   (background_view venue_select)
   (focus selvenue_panel)
   (back song_select_screen)
   (helpbar
      ((cancel helpbar_back)
         (confirm helpbar_select)))}