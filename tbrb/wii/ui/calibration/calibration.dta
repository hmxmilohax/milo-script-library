#define CAL_AUTO_PANEL
((state init)
   (old_lag 0)
   (confirmed FALSE)
   (set_state
      ($s)
      {set
         [state]
         $s}
      {switch
         [state]
         (init
            {cal_audio_instructions.lbl set showing TRUE}
            {cal_video_instructions.lbl set showing TRUE}
            {cal_instructions.lbl set text_token lag_info_why}
            {$this populate_setting_label})
         (calibrating
            {platform_mgr disable_xmp}
            #ifndef HX_WII
            {cal_instructions.lbl set text_token lag_measuring}
            #else
            {cal_instructions.lbl
               set
               text_token
               ""}
            #endif
            {cal_setting.lbl
               set
               text_token
               ""}
            {helpbar
               set_config
               ((cancel helpbar_back))})
         (success
            {cal_audio_instructions.lbl set showing FALSE}
            {cal_video_instructions.lbl set showing FALSE}
            {cal_instructions.lbl set text_token lag_success}
            {$this populate_setting_label}
            {helpbar
               set_config
               ((cancel helpbar_back)
                  (confirm helpbar_select))})
         (failure
            {if_else
               [is_hardware]
               {do
                  {cal_failed_screen
                     set_return_screen
                     {ui current_screen}}
                  {if_else
                     [is_video]
                     {cal_failed_screen set_failure_message cal_video_fail_calbert}
                     {cal_failed_screen set_failure_message cal_audio_fail_calbert}}
                  {ui push_screen cal_failed_screen}}
               {do
                  {cal_manual_failed_screen
                     set_retry_screen
                     {ui current_screen}}
                  {if_else
                     [is_video]
                     {cal_manual_failed_screen set_numbers_screen cal_manual_video_postfailure_screen}
                     {cal_manual_failed_screen set_numbers_screen cal_manual_audio_postfailure_screen}}
                  {ui push_screen cal_manual_failed_screen}}}
            {$this populate_setting_label})})
   (populate_setting_label
      {if_else
         [is_video]
         {cal_setting.lbl
            set_token_fmt
            (current_lag_setting
               {int
                  {floor
                     {'+'
                        0.5
                        {profile_mgr get_excess_video_lag}}}})}
         {cal_setting.lbl
            set_token_fmt
            (current_lag_setting
               {int
                  {floor
                     {'+'
                        0.5
                        {profile_mgr get_excess_audio_lag}}}})}})
   (enter
      {$this init_panel}
      {$this
         set_focus
         ""}
      #ifdef HX_WII
      {rnd frames_buffered 2}
      #endif)
   (init_panel
      {$this add_sink cal_background_panel}
      {$this set_state init}
      {set
         [max_slack]
         75}
      {set
         [anim_cycle_frames]
         50}
      {set
         [anim_num_cycles]
         2}
      {set
         [anim_resting_frame]
         0}
      {set
         [anim_half_off]
         FALSE}
      {set
         [confirmed]
         FALSE}
      {if_else
         [is_video]
         {do
            {set
               [cycle_time_ms]
               605}
            {set
               [old_lag]
               {profile_mgr get_excess_video_lag}}
            {set
               [video_enable]
               TRUE}
            {set
               [audio_enable]
               FALSE}}
         {do
            {set
               [cycle_time_ms]
               605}
            {set
               [old_lag]
               {profile_mgr get_excess_audio_lag}}
            {set
               [video_enable]
               FALSE}
            {set
               [audio_enable]
               TRUE}}}
      {set
         [hardware_mode]
         FALSE}
      {set
         [num_hits]
         32}
      {set
         [bottom_outliers]
         8}
      {set
         [top_outliers]
         4}
      {if
         [is_hardware]
         {set
            [num_hits]
            24}
         {set
            [hardware_mode]
            TRUE}
         {set
            [cycle_time_ms]
            555}
         {set
            [bottom_outliers]
            3}
         {set
            [top_outliers]
            3}})
   (TRANSITION_COMPLETE_MSG
      {$this initialize_content})
   (exit
      {if
         {!
            [confirmed]}
         {if_else
            [is_video]
            {do
               {profile_mgr
                  set_excess_video_lag
                  [old_lag]}}
            {do
               {profile_mgr
                  set_excess_audio_lag
                  [old_lag]}}}}
      #ifdef HX_WII
      {rnd frames_buffered 1}
      #endif)
   (BUTTON_DOWN_MSG
      {if_else
         {&&
            {!=
               [state]
               failure}
            {!=
               [state]
               calibrating}
            {!
               [confirmed]}}
         {switch
            $action
            (kAction_Confirm
               {$this set_state calibrating}
               {$this start_countdown $user})
            kDataUnhandled}
         {if_else
            {&&
               {== $action kAction_Cancel}
               {==
                  [state]
                  success}}
            {do
               {play_instr_sfx $user button_back}
               {if_else
                  [is_video]
                  {do
                     {profile_mgr
                        set_excess_video_lag
                        [old_lag]}}
                  {do
                     {profile_mgr
                        set_excess_audio_lag
                        [old_lag]}}}
               {$this init_panel}
               {$this initialize_content}
               0}
            {if_else
               {&&
                  {==
                     [state]
                     calibrating}
                  {== $action kAction_Confirm}}
               0
               kDataUnhandled}}})
   (start_countdown
      ($user)
      {thread_task
         kTaskUISeconds
         (script
            {set
               [confirmed]
               FALSE}
            {cal_instructions.lbl set showing FALSE}
            {$this start_test $user}
            {$task
               wait
               {$this is_processing_input}}
            {if_else
               {$this get_test_quality}
               {do
                  {if_else
                     [is_video]
                     {do
                        {profile_mgr
                           set_excess_video_lag
                           {$this get_test_result}}}
                     {do
                        {profile_mgr
                           set_excess_audio_lag
                           {$this get_test_result}}}}
                  {$this set_state success}
                  {set
                     [confirmed]
                     TRUE}}
               {$this set_state failure}}
            {cal_instructions.lbl set showing TRUE})}))
