                  {cal_welcome_screen get first_time}
                  {do
                     {cal_welcome_screen set first_time FALSE}
                     {set $target_screen meta_loading_splash_to_main_menu}
                     {content_mgr start_refresh}
                     {startup_sdmode_begin_dialog set final_screen $target_screen}
                     {ui goto_screen startup_sdmode_begin_dialog}}
                  {ui goto_screen $target_screen}}
               #else
               {ui goto_screen meta_loading_splash_to_main_menu}
               #endif})
         kDataUnhandled})}
