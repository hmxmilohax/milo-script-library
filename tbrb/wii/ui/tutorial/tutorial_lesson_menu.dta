{new
   UIPanel
   tutorials_lesson_menu_panel
   (file
      "tutorial_level_menu_keep.milo")
   (goto_lesson
      ($num)
      {{gamemode get_main_panel}
         set_lesson_num
         $num}
      {ui goto_screen tutorials_loading_screen})
   (enter
      {$this set_focus lesson_1.btn}
      {foreach_int
         $i
         0
         {size
            [buttons]}
         {{elem
               [buttons]
               $i}
            set_showing
            {$i
               <
               {gamemode get_main_panel}
               get_num_levels}}
         {{elem
               [buttons]
               $i}
            set_state
            {if_else
               {$i
                  <
                  {gamemode get_main_panel}
                  get_num_levels}
               kNormal
               kDisabled}}
         {{elem
               [check_boxes]
               $i}
            set_showing
            {$i
               <
               {gamemode get_main_panel}
               get_num_levels}}
         {{elem
               [check_boxes]
               $i}
            set_showing
            {&&
               {{profile_mgr
                     get_profile
                     {session_mgr get_leader_user}}
                  is_lesson_complete
                  $i}
               {$i
                  <
                  {gamemode get_main_panel}
                  get_num_levels}}}})
   (exit)
   (TRANSITION_COMPLETE_MSG
      {if
         {&&
            {!
               {ui in_transition}}
            {==
               {ui current_screen}
               tutorials_lesson_menu_screen}
            {!
               {ui_event_mgr has_active_transition_event}}}
         {autosave}})
   (SELECT_MSG
      {set
         [lesson_num]
         0}
      {$this
         goto_lesson
         [lesson_num]})
   (buttons lesson1.btn lesson2.btn lesson3.btn)
   (checks check1.mesh check2.mesh check3.mesh)
   (check_boxes checkbox1.mesh checkbox2.mesh checkbox3.mesh)}
{new
   BandScreen
   tutorials_lesson_menu_screen
   (panels meta tutorials_lesson_menu_panel)
   (focus tutorials_lesson_menu_panel)
   (back MAIN_MENU_RETURN_SCREEN)
   (enter
      {input_mgr set_limit kLimitSession}
      {meta music_start})
   (helpbar
      ((cancel helpbar_back)
         (confirm helpbar_select)))}