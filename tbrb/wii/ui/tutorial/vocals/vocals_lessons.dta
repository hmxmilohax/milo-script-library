{new
   TutorialLesson
   vocals_singing_lesson
   (begin)
   (end
      {{gamemode get_main_panel}
         tut_show
         top_phrase_feedback}
      {{gamemode get_main_panel}
         tut_show
         bottom_phrase_feedback}
      {{gamemode get_main_panel}
         modify_guide_pitch
         0})
   (title vocals1_lesson1_basics)
   (intro_vo tute_vocal_howintro)
   (song vocals_basics_singing)
   (timeline singing_timeline)
   (challenge vocals_singing_challenge)
   (challenge_timeout 60.0)
   (challenge_finish_time 2.0)
   (task_text vocals1_lesson1_basics_task)
   (success_text
      (vocals1_lesson1_complete))
   (success_vo
      (tute_vocal_howwin))
   (fail_text vocals1_lesson1_fail)
   (fail_vo tute_vocal_howfail)
   (uses_harmony FALSE)
   (can_skip TRUE)}
