{new
   TutorialChallenge
   vocals_singing_challenge
   (enter
      {{beatmatch active_player 0}
         add_sink
         $this})
   (exit
      {{beatmatch active_player 0}
         remove_sink
         $this}
      {{gamemode get main_panel}
         tut_track_out})
   (phrase_end)
   (is_running TRUE)
   (success
      {>=
         {{beatmatch active_player 0}
            get_hit_percentage
            0}
         [success_percent]})
   (success_level 0)
   (success_percent 0.5)}
{new
   TutorialChallenge
   vocals_nonpitched_challenge
   (enter
      {set
         [last_percent_hit]
         0.0}
      {{beatmatch active_player 0}
         add_sink
         $this})
   (exit
      {{beatmatch active_player 0}
         remove_sink
         $this})
   (phrase_end
      {if
         {>
            {$this percent_hit}
            [last_percent_hit]}
         {set
            [last_percent_hit]
            {$this percent_hit}}})
   (percent_hit
      {'*'
         100.0
         {/
            {{beatmatch active_player 0}
               score}
            1200}})
   (is_running TRUE)
   (success
      {>=
         {$this percent_hit}
         50.0})
   (success_level 0)
   (last_percent_hit 0.0)}
{new
   TutorialChallenge
   vocals_overdrive_challenge
   (enter
      {set
         [deployed]
         FALSE}
      {{beatmatch active_player 0}
         add_sink
         $this}
      {{gamemode get main_panel}
         tut_set_overdrive
         0.0})
   (exit
      {{beatmatch active_player 0}
         remove_sink
         $this}
      {{gamemode get main_panel}
         tut_track_out}
      {{gamemode get main_panel}
         tut_set_overdrive
         0.0})
   (unison_hit)
   (deploy
      {set
         [deployed]
         TRUE})
   (is_running
      {!
         [deployed]})
   (success
      [deployed])
   (success_level 0)
   (deployed FALSE)}
{new
   TutorialChallenge
   vocals_harmony_guide_challenge
   (enter
      {{gamemode get main_panel}
         tut_enable_guide_track
         [part]}
      {{gamemode get main_panel}
         tut_isolate_vocal_track
         [part]}
      {{gamemode get main_panel}
         autoplay_part
         0}
      {{gamemode get main_panel}
         autoplay_part
         2}
      {{beatmatch active_player 0}
         add_sink
         $this}
      {set
         [phrases_hit]
         0}
      {{gamemode get main_panel}
         tut_set_challenge_score
         [phrases_hit]
         [phrases_to_hit]}
      {{gamemode get main_panel}
         block_mics})
   (exit
      {{beatmatch active_player 0}
         remove_sink
         $this}
      {{gamemode get main_panel}
         tut_enable_guide_track
         -1}
      {{gamemode get main_panel}
         autoplay_part
         -1}
      {{gamemode get main_panel}
         tut_track_out}
      {{gamemode get main_panel}
         unblock_mics})
   (phrase_rating
      ($rating)
      {if
         {>= $rating 5}
         {'++'
            [phrases_hit]}
         {{gamemode get main_panel}
            tut_set_challenge_score
            [phrases_hit]
            [phrases_to_hit]}})
   (is_running
      {&&
         {!
            {$this success}}})
   (success
      {>=
         [phrases_hit]
         [phrases_to_hit]})
   (success_level 0)
   (part 1)
   (phrases_hit 0)
   (phrases_to_hit 3)}
{new
   TutorialChallenge
   vocals_harmony_challenge
   (enter
      {{beatmatch active_player 0}
         add_sink
         $this}
      {{gamemode get main_panel}
         tut_enable_guide_track
         -1}
      {{gamemode get main_panel}
         tut_isolate_vocal_track
         1}
      {{gamemode get main_panel}
         autoplay_part
         0}
      {{gamemode get main_panel}
         autoplay_part
         2}
      {set
         [phrases_hit]
         0}
      {{gamemode get main_panel}
         tut_set_challenge_score
         [phrases_hit]
         [phrases_to_hit]}
      {{gamemode get main_panel}
         block_mics})
   (exit
      {{beatmatch active_player 0}
         remove_sink
         $this}
      {{gamemode get main_panel}
         autoplay_part
         -1}
      {{gamemode get main_panel}
         tut_track_out}
      {{gamemode get main_panel}
         unblock_mics})
   (phrase_rating
      ($rating)
      {if
         {>= $rating 5}
         {'++'
            [phrases_hit]}
         {{gamemode get main_panel}
            tut_set_challenge_score
            [phrases_hit]
            [phrases_to_hit]}})
   (is_running
      {&&
         {!
            {$this success}}
         {<
            {{beatmatch active_player 0}
               percent_complete}
            99.0}})
   (success
      {>=
         [phrases_hit]
         [phrases_to_hit]})
   (success_level 0)
   (phrases_hit 0)
   (phrases_to_hit 3)}
{new
   TutorialChallenge
   vocals_harmony_scoring_challenge
   (enter
      {{gamemode get main_panel}
         autoplay_part
         0}
      {{gamemode get main_panel}
         autoplay_part
         2}
      {{beatmatch active_player 0}
         add_sink
         $this}
      {{gamemode get main_panel}
         block_mics}
      {set
         [win]
         FALSE}
      {set
         [phrases_hit]
         0}
      {{gamemode get main_panel}
         tut_set_challenge_score
         [phrases_hit]
         [phrases_to_hit]})
   (exit
      {{gamemode get main_panel}
         autoplay_part
         -1}
      {{beatmatch active_player 0}
         remove_sink
         $this}
      {{gamemode get main_panel}
         tut_track_out}
      {{gamemode get main_panel}
         unblock_mics})
   (phrase_rating
      ($rating)
      {if
         {>= $rating 5}
         {'++'
            [phrases_hit]}
         {{gamemode get main_panel}
            tut_set_challenge_score
            [phrases_hit]
            [phrases_to_hit]}}
      {if
         {>=
            [phrases_hit]
            [phrases_to_hit]}
         {set
            [win]
            TRUE}})
   (is_running
      {!
         [win]})
   (success
      [win])
   (phrases_hit 0)
   (phrases_to_hit 2)
   (success_level 0)
   (win FALSE)}
{new
   TutorialChallenge
   vocals_mic_screen_challenge
   (enter
      {vocals_mic_status_panel add_sink $this}
      {set
         [made_sound]
         FALSE})
   (exit
      {vocals_mic_status_panel remove_sink $this})
   (mic_arrow_update
      ($alpha $mic_id)
      {if
         {&&
            {!
               [made_sound]}
            {>= $alpha 0.5}}
         {set
            [made_sound]
            TRUE}
         {set
            [sound_start]
            {{gamemode get main_panel}
               tutorial_seconds}}
         {{gamemode get main_panel}
            set_mic_id
            $mic_id}})
   (is_running
      {!
         {&&
            [made_sound]
            {>=
               {-
                  {{gamemode get main_panel}
                     tutorial_seconds}
                  [sound_start]}
               [sound_time]}}})
   (success
      [made_sound])
   (success_level 0)
   (made_sound FALSE)
   (sound_start 0.0)
   (sound_time 1.0)}
{new
   TutorialChallenge
   vocals_dummy_challenge
   (enter)
   (exit)
   (success TRUE)
   (success_level 0)}