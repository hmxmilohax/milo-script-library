{new
   TutorialChallenge
   drum_beat1_challenge
   (enter
      {{beatmatch active_player 0}
         add_sink
         $this}
      {{{get_track_panel}
            get_first_track}
         enable_slot
         KICK_SLOT}
      {{{get_track_panel}
            get_first_track}
         enable_slot
         SNARE_SLOT}
      {{{get_track_panel}
            get_first_track}
         disable_slot
         HIHAT_SLOT}
      {set
         [state]
         0}
      {foreach_int
         $i
         0
         {size
            [gems_hit]}
         {set_elem
            [gems_hit]
            $i
            0}}
      {$this update_score}
      {{{gamemode get main_panel}
            get_lesson}
         set
         fail_vo
         tute_drum2_kicksnarefail}
      {set
         [enable_hit]
         TRUE})
   (exit
      {{beatmatch active_player 0}
         remove_sink
         $this}
      {{{get_track_panel}
            get_first_track}
         enable_slot
         KICK_SLOT}
      {{{get_track_panel}
            get_first_track}
         enable_slot
         SNARE_SLOT}
      {{{get_track_panel}
            get_first_track}
         enable_slot
         HIHAT_SLOT}
      {{gamemode get main_panel}
         tut_track_out})
   (hit
      {if
         [enable_hit]
         {set_elem
            [gems_hit]
            [state]
            {'+'
               1
               {elem
                  [gems_hit]
                  [state]}}}
         {$this update_score}
         {if_else
            {==
               [state]
               0}
            {if
               {==
                  {elem
                     [gems_hit]
                     0}
                  {elem
                     [gems_to_hit]
                     0}}
               {$this next_state}}
            {if
               {==
                  {elem
                     [gems_hit]
                     1}
                  {elem
                     [gems_to_hit]
                     1}}
               {set
                  [enable_hit]
                  FALSE}}}})
   (mistake
      {if
         [enable_hit]
         {set_elem
            [gems_hit]
            [state]
            0}
         {$this update_score}})
   (miss
      {$this mistake})
   (pass
      {$this mistake})
   (update_score
      {{gamemode get main_panel}
         tut_set_challenge_score
         {elem
            [gems_hit]
            [state]}
         {elem
            [gems_to_hit]
            [state]}})
   (next_state
      {{gamemode get main_panel}
         play_vo
         tute_drum2_kicksnarewin}
      {{{gamemode get main_panel}
            find
            task_text.lbl}
         set
         text_token
         drum2_lesson1_beat_task2}
      {set
         [state]
         1}
      {set
         [enable_hit]
         FALSE}
      {{{gamemode get main_panel}
            get_lesson}
         set
         fail_vo
         tute_drum2_allfail}
      {script_task
         kTaskSeconds
         (delay 7)
         (script
            {$this enable_everything})})
   (enable_everything
      {{{get_track_panel}
            get_first_track}
         enable_slot
         KICK_SLOT}
      {{{get_track_panel}
            get_first_track}
         enable_slot
         SNARE_SLOT}
      {{{get_track_panel}
            get_first_track}
         enable_slot
         HIHAT_SLOT}
      {set
         [enable_hit]
         TRUE}
      {$this update_score})
   (is_running
      {!
         {$this success}})
   (success
      {&&
         {>=
            {elem
               [gems_hit]
               0}
            {elem
               [gems_to_hit]
               0}}
         {>=
            {elem
               [gems_hit]
               1}
            {elem
               [gems_to_hit]
               1}}})
   (success_level 0)
   (gems_hit
      (0 0))
   (gems_to_hit
      (10 12))
   (enable_hit TRUE)
   (state 0)}
