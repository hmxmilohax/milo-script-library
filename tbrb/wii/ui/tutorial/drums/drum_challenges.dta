{new
   TutorialChallenge
   drum_thrash_challenge
   (enter
      {{gamemode get main_panel}
         add_drum_hit_callback
         $this}
      {{gamemode get main_panel}
         enable_controller}
      {{gamemode get main_panel}
         set_sfx_volume
         0.0}
      {foreach_int
         $i
         0
         {size
            [pad_count]}
         {set_elem
            [pad_count]
            $i
            0}})
   (exit
      {{gamemode get main_panel}
         remove_drum_hit_callback
         $this})
   (drum_hit
      ($pad_num)
      {if_else
         {{{session_mgr get_leader_user}
               get_gameplay_options}
            get_lefty}
         {do
            {if_else
               {== $pad_num 0}
               {set $sample_num 0}
               {set
                  $sample_num
                  {- 5 $pad_num}}}}
         {do
            {set $sample_num $pad_num}}}
      {{{gamemode get main_panel}
            find
            [sample_names $sample_num]}
         play}
      {{{gamemode get main_panel}
            find
            {elem
               [hit_triggers]
               $pad_num}}
         trigger}
      {if
         {<
            {elem
               [pad_count]
               $pad_num}
            [num_plays]}
         {set_elem
            [pad_count]
            $pad_num
            {'+'
               1
               {elem
                  [pad_count]
                  $pad_num}}}
         {if
            {==
               {elem
                  [pad_count]
                  $pad_num}
               [num_plays]}
            {{{gamemode get main_panel}
                  find
                  {elem
                     [triggers]
                     $pad_num}}
               trigger}}})
   (is_running
      {!
         {$this success}})
   (success
      {&&
         {>=
            {elem
               [pad_count]
               0}
            [num_plays]}
         {>=
            {elem
               [pad_count]
               1}
            [num_plays]}
         {>=
            {elem
               [pad_count]
               2}
            [num_plays]}
         {>=
            {elem
               [pad_count]
               3}
            [num_plays]}
         {>=
            {elem
               [pad_count]
               4}
            [num_plays]}})
   (success_level 0)
   (pad_count
      (0 0 0 0 0))
   (num_plays 4)
   (hit_triggers
      (c0_hit0.trig c0_hit1.trig c0_hit2.trig c0_hit3.trig c0_hit4.trig))
   (triggers
      (c0_pad0.trig c0_pad1.trig c0_pad2.trig c0_pad3.trig c0_pad4.trig))
   (sample_names
      (ds1005.cue ds1001.cue ds1002.cue ds1003.cue ds1004.cue))}
{new
   TutorialChallenge
   drum_onehand_challenge
   (enter
      {{beatmatch active_player 0}
         add_sink
         $this}
      {set
         [perfect]
         TRUE}
      {set
         [num_gems_hit]
         0}
      {{gamemode get main_panel}
         tut_set_challenge_score
         [num_gems_hit]
         [gems_to_hit]})
   (exit
      {{beatmatch active_player 0}
         remove_sink
         $this}
      {{gamemode get main_panel}
         tut_track_out})
   (hit
      {if
         {<
            [num_gems_hit]
            [gems_to_hit]}
         {'++'
            [num_gems_hit]}
         {{gamemode get main_panel}
            tut_set_challenge_score
            [num_gems_hit]
            [gems_to_hit]}})
   (miss
      {set
         [perfect]
         FALSE})
   (pass
      {set
         [perfect]
         FALSE})
   (is_running
      {!
         {$this success}})
   (success
      {>=
         [num_gems_hit]
         [gems_to_hit]})
   (success_level
      {if_else
         [perfect]
         1
         0})
   (num_gems_hit 0)
   (gems_to_hit 8)
   (perfect TRUE)}
