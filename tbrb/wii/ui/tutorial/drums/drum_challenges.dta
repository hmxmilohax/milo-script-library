{new
   TutorialChallenge
   drum_deploy_challenge
   (enter
      {{beatmatch active_player 0}
         add_sink
         $this}
      {set
         [deployed]
         FALSE})
   (exit
      {{beatmatch active_player 0}
         remove_sink
         $this}
      {{gamemode get main_panel}
         tut_track_out})
   (note_seen
      ($id)
      {if
         {&&
            {'||'
               {== $id 38}
               {== $id 62}}
            {!
               [deployed]}}
         {{gamemode get main_panel}
            play_vo
            tute_drum2_deployhelp}})
   (deploy
      {set
         [deployed]
         TRUE})
   (pass
      ($id)
      {$this note_seen $id})
   (hit
      ($slot $id)
      {$this note_seen $id})
   (is_running
      {!
         [deployed]})
   (success
      [deployed])
   (success_level 0)
   (deployed FALSE)}