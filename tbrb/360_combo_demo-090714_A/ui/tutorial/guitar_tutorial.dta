#include guitar/guitar_levels.dta
#include guitar/guitar_challenges.dta
#include guitar/guitar_lessons.dta
{new GuitarTutorialPanel
   guitar_tutorial_panel
   (file
      {if_else {{{session_mgr get_leader_user} get_gameplay_options} get_lefty}
         "guitar_tutorial_panel_lefty.milo"
         "guitar_tutorial_panel.milo"
      }
   )
   (levels (guitar_level_0 guitar_level_1 guitar_level_2))
   (lessons
      (guitar_frets_lesson
         guitar_notes_lesson
         guitar_diff_notes_lesson
         guitar_held_notes_lesson
         guitar_chord_lesson
         guitar_ui_lesson
         guitar_consecutive_lesson
         guitar_overdrive_lesson
         guitar_whammy_lesson
         guitar_deploy_lesson
         guitar_solo_lesson
         guitar_fingering_lesson
         guitar_trills_lesson
         guitar_hopos_lesson
         guitar_mixed_lesson
      )
   )
   #include guitar/guitar_timelines.dta
   (enter TUTORIAL_PANEL_ENTER)
   (exit TUTORIAL_PANEL_EXIT)
   TUTORIAL_PANEL_LESSON_HANDLERS
   TUTORIAL_PANEL_STATE_HANDLERS
   TUTORIAL_PANEL_PROPERTIES
   TUTORIAL_PANEL_HANDLERS
   TUTORIAL_PANEL_COMMAND_HANDLERS
   (handle_new_song {{{get_track_panel} get_first_track} handle_new_song})
   (get_timeline_file "ui/tutorial/guitar/guitar_timelines.dta")
   (tutorial_type guitar_tutorial)
}
{new BandScreen
   guitar_tutorial_screen
   (panels
      tutorial_background_panel
      game
      coop_track_panel
      guitar_tutorial_panel
   )
   (focus guitar_tutorial_panel)
   TUTORIALS_MAIN_SCREEN_GENERAL_PROPERTIES
   (enter)
   (exit TUTORIAL_SCREEN_EXIT)
}