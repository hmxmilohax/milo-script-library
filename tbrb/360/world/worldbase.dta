            {$ww copy Wind.wind kCopyDeep}}}})
#define WORLD_SETUP_CHARACTERS
(SET_CHAR_ENVS
   {unless
      $p9director
      {if
         {&&
            {has_substr
               {path_name $this}
               "greenscreen/scene"}
