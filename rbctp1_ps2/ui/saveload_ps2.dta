{new
   UIScreen
   save_screen
   SAVELOAD_DIALOG_SCREEN
   (overwrite 0)
   (enter_time 0.0)
   (save_time 0.0)
   (done_saving FALSE)
   (done_polling FALSE)
   (long FALSE)
   (enter
      {if
         {helpbar is_up}
         {helpbar hide_helpbar}}
      {set
         [enter_time]
         {taskmgr ui_seconds}}
      {set
         [save_time]
         0.0}
      {set
         [done_saving]
         FALSE}
      {set
         [done_polling]
         FALSE}
      {$this
         setup_msg_localized
         {sprintf
            {localize_token mc_save_ps2}
            {memcardmgr get_memcard_slot_name}}}
      {$this set_title mc_title_saving}
      {memcardmgr
         save_game
         $saveload_player_num
         [overwrite]})
   (exit
      {if
         {helpbar is_up}
         {helpbar unhide_helpbar}})
   (MEMCARD_RESULT_MSG
      {switch
         $result
         (kMCDifferentCard
            {set
               [done_polling]
               TRUE}
            {ui goto_screen auto_save_different_card_screen})
         (kMCNoError
            {set
               [done_saving]
               TRUE}
            {set
               [save_time]
               {taskmgr ui_seconds}}
            {ui goto_screen save_complete_screen})
         (kMCFileExists
            {set
               [done_polling]
               TRUE}
            {ui goto_screen save_confirm_overwrite_screen})
         (kMCNotEnoughSpace
            {set
               [done_polling]
               TRUE}
            {ui goto_screen save_not_enough_space_screen})
         (kMCNoCard
            {set
               [done_polling]
               TRUE}
            {ui goto_screen auto_save_storage_missing_screen})
         (kMCNotFormatted
            {ui goto_screen error_not_formatted_screen})
         {set
            [done_polling]
            TRUE}
         {ui goto_screen save_failed_screen}})}
