{dialog 'Search and Rename'
   {milo cur_dir}
   (editor
      (find string (help "String to find"))
      (replace string (help "String to replace with"))
      (ok script (script {$this on_ok}))
   )
   (find "")
   (replace "")
   (on_ok
      {set $objs {array 0}}
      {{milo cur_dir} iterate
         Object
         $obj
         {if {has_substr {$obj name} [find]}
            {insert_elem $objs 0 $obj}
         }
      }
      {foreach $obj $objs
         {search_replace {$obj name} [find] [replace] $out}
         {milo record $obj 'set name'}
         {$obj set_name $out}
      }
      {set $objs 0}
      {milo update_objects}
   )
}