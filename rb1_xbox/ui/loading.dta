#ifndef _SHIP
{new
   UIScreen
   loading_multi_screen
   (enter
      {ui goto_screen loading_multi_content_screen})}
{new
   UIPanel
   loading_multi_content_panel
   (load
      {content_mgr start_refresh})
   (is_loaded
      {content_mgr refresh_done})}
{new
   UIScreen
   loading_multi_content_screen
   (panels loading_multi_content_panel)
   (enter
      {tour create_test_event}
      {game
         set_song
         {{tour performer}
            next_song}}
      {ui goto_screen loading_screen})}
{new
   UIPanel
   loading_mem_chars
   (load
      {game load_all_chars TRUE})
   (is_loaded
      {char_cache loaded})}
{new
   UIScreen
   loading_mem_screen
   (panels loading_mem_chars)
   (enter
      {heap_report}
      {ui goto_screen loading_screen})}
#endif
{new
   UIPanel
   loading_panel
   (file
      "loading.milo")
   (next_screen '')
   (enter
      {seed_random_context
         {gamecfg get net_random_seed}}
      {set
         [next_screen]
         {cond
            ($force_loading_tip loading_tip_screen)
            ($force_loading_image loading_image_screen)
            ({&&
                  {gamemode get show_loading_images}
                  {==
                     {mod
                        {gamecfg get net_random_seed}
                        2}
                     0}}
               loading_image_screen)
            (TRUE loading_tip_screen)}}
      {if_else
         {==
            [next_screen]
            loading_tip_screen}
         {do
            {Plane01.mesh set_showing TRUE}
            {tip.lbl set_showing TRUE}
            {tip.lbl
               set_localized
               {localize
                  {if_else
                     {gamemode get use_static_tip}
                     {gamemode get the_static_tip}
                     {random_context
                        {elem
                           {find $syscfg tips}
                           1}}}}}}
         {do
            {Plane01.mesh set_showing FALSE}
            {tip.lbl set_showing FALSE}}})
   (load
      {gamecfg set_active_roster TRUE}
      {band_ui_sink set_block_mode kBlockLoadingGame})
   (unload
      {band_ui_sink set_block_mode kBlockNone})}
{new
   UIPanel
   wait_on_content_mgr_panel
   (load
      {content_mgr start_refresh})
   (is_loaded
      {content_mgr refresh_done})}
{new
   UIScreen
   loading_screen
   (panels loading_panel wait_on_content_mgr_panel)
   (enter
      {band_ui_sink set_net_sync FALSE}
      {ui goto_screen preloading_screen})}
{new
   PreloadPanel
   preload_panel
   (content_name
      "")
   (corrupt FALSE)
   (enter
      {if_else
         [preload_ok]
         {ui goto_screen real_loading_screen}
         {do
            {band_ui_sink set_block_mode kBlockNone}
            {band_ui_sink
               content_file_read_failed
               [content_name]
               [corrupt]}}})}
{new
   UIScreen
   preloading_screen
   (panels loading_panel preload_panel)
   (focus preload_panel)}
{new
   UIScreen
   real_loading_screen
   (panels loading_panel)
   (enter
      {song_provider
         add_recent_song
         {gamecfg get_song}}
      {ui
         goto_screen
         {loading_panel get next_screen}})}
{new
   UIPanel
   char_loading_panel
   (is_loaded
      {char_cache loaded})
   (load
      {if
         {gamemode get load_chars}
         {game load_all_chars TRUE}})}
{new
   UIPanel
   loading_instrument
   (file loading_instrument.milo kLoadStayBack)}
{new
   UIScreen
   loading_tip_screen
   (panels loading_panel)
   (enter
      {synth stop_all_sfx}
      {ui goto_screen loading_tip_screen_2})}
{new
   UIScreen
   loading_tip_screen_2
   (panels loading_panel char_loading_panel)
   (enter
      {if
         {gamemode get load_chars}
         {char_cache load_instruments}}
      {ui goto_screen loading_tip_screen_3})}
{new
   UIScreen
   loading_tip_screen_3
   (panels loading_panel loading_instrument)
   (enter
      {ui
         goto_screen
         {gamemode get game_screen}}
      {band_ui_sink set_net_sync TRUE})}
{new
   UIScreen
   loading_image_screen
   (panels loading_panel char_loading_panel)
   (enter
      {synth stop_all_sfx}
      {if
         {gamemode get load_chars}
         {char_cache load_instruments}}
      {ui goto_screen loading_instrument_screen})}
{new
   UIScreen
   loading_instrument_screen
   (panels loading_instrument loading_panel)
   (enter
      {ui goto_screen loading_photo_screen})}
{new
   LoadingPanel
   loading_photo
   (file
      {if_else
         $force_loading_image
         $force_loading_image
         {if_else
            {gamemode get load_chars}
            {if_else
               {gamemode is_band_arrangement}
               {random_context
                  {elem
                     {find $syscfg loading_screens}
                     1}}
               {switch
                  {{game
                        get_player_config
                        {game get leader_num}}
                     get_track_sym}
                  (guitar loading/ls_h2h_guitar.milo)
                  (bass loading/ls_h2h_bass.milo)
                  (drum loading/ls_h2h_drum.milo)
                  (vocals loading/ls_h2h_vocals.milo)}}
            ""}}
      kLoadStayBack)}
{new
   UIScreen
   loading_photo_screen
   (panels loading_photo loading_panel)
   (delay_frames -1)
   (enter
      {set
         [delay_frames]
         3})
   (poll
      {--
         [delay_frames]}
      {if
         {==
            [delay_frames]
            0}
         {set
            [delay_frames]
            -1}
         {game load_all_chars TRUE}
         {ui
            goto_screen
            {gamemode get game_screen}}
         {band_ui_sink set_net_sync TRUE}})}